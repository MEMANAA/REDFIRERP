CMD:members(playerid, params[])
{
	new teamnumber;
	if (PlayerInfo[playerid][pLeader] != 0)
	{
		teamnumber = PlayerInfo[playerid][pLeader];
	}
	else if (PlayerInfo[playerid][pMember] != 0)
	{
		teamnumber = PlayerInfo[playerid][pMember];
	}
	else
	{
		return SCM(playerid, COLOR_GRAD1, " Вам недоступна эта функция!");
	}
	SCM(playerid, COLOR_YELLOW, " Члены организации Онлайн:");
	if(!IsAMafia(playerid) && !IsAGang(playerid)) SCM(playerid, TEAM_GROVE_COLOR, " На работе:");
	new countmute = 0, stringr[128];
	foreach(new i: Player)
	{
		if ((PlayerInfo[i][pMember] == teamnumber) && IsPlayerConnected(i))
		{
		    if(!PlayerInfo[i][pLogin] || PlayerInfo[i][pAdmin]) continue;
			countmute++;
			if (PlayerInfo[i][pMember] == teamnumber && IsAMafia(playerid) || IsAGang(playerid))
			{
				if(GetPVarInt(i, "AFK_Time") > 2) format(stringr, sizeof(stringr), " ID: %d | %s %s[%d] | {A14599}[AFK]: %s", i, Name(i), f_rank[PlayerInfo[i][pMember] - 1][PlayerInfo[i][pRank] - 1], PlayerInfo[i][pRank], Convert(GetPVarInt(i,"AFK_Time")-2));
				else format(stringr, sizeof(stringr), " ID: %d | %s %s[%d] | Часов во фракции: %d", i, Name(i), f_rank[PlayerInfo[i][pMember] - 1][PlayerInfo[i][pRank] - 1], PlayerInfo[i][pRank], PlayerInfo[i][pMemberTime]/3600);
			}
			else if (PlayerInfo[i][pMember] == teamnumber && GetPVarInt(i, "Fraction_Duty"))
			{
			    if(GetPVarInt(i, "AFK_Time") > 2) format(stringr, sizeof(stringr), " ID: %d | %s %s[%d] | {A14599}[AFK]: %s", i, Name(i), GetRank(i), PlayerInfo[i][pRank], Convert(GetPVarInt(i,"AFK_Time")-2));
				else format(stringr, sizeof(stringr), " ID: %d | %s %s[%d] | Часов во фракции: %d | Отыграно за день: %s", i, Name(i), GetRank(i), PlayerInfo[i][pRank], PlayerInfo[i][pMemberTime]/3600, ConvertTime(PlayerInfo[i][pGameTime]));
			}
			else if (PlayerInfo[i][pMember] == teamnumber && !GetPVarInt(i, "Fraction_Duty"))
			{
				format(stringr, sizeof(stringr), " ID: %d | %s %s[%d] | {ff091c}[Выходной]", i, Name(i), GetRank(i), PlayerInfo[i][pRank]);
			}
			if (strlen(stringr) > 1)
			{
                SCM(playerid, 0xDEA002FF, stringr);
			}
		}
	}
	if (countmute == 0)
	{
		SCM(playerid, COLOR_GRAD1, " Нет игроков в сети!");
	}
	else
	{
		SCMF(playerid, COLOR_YELLOW, " Всего онлайн: %d", countmute);
	}
	return true;
}
