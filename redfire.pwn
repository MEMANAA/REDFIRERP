WriteLog(const file[],stringa[])
{
	new write[256],minute,second,hour,day,month,year;
	gettime(hour,minute,second);
	getdate(year,month,day);
	if(strlen(file)>0)format(write, sizeof(write), "gamefiles/%s",file); else format(write, sizeof(write), "gamefiles/%04.d-%02.d-%02.d.log",year,month,day);
	new File:hFile = fopen(write, io_append);
	format(write, sizeof(write), "[%d.%02d.%02d | %02d:%02d:%02d] %s\r\n",day,month,year,hour,minute,second,stringa);
	for(new i; i < strlen(write); i ++) fputchar(hFile, write[i], false);
	fclose(hFile);
}
