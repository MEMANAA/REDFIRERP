	cache_get_row_count(r);
	if(!r) print("[Chatvirtva]: Chatvirtva Ver Moxerxda GangZone!");
	for(new i = 0; i < 130; i++)
	{
	    cache_get_value_name_int(i, "gzID", GZInfo[i][gzID]);
	    cache_get_value_name_float(i, "gzX", GZInfo[i][gCoords][0]);
	    cache_get_value_name_float(i, "gzY", GZInfo[i][gCoords][1]);
	    cache_get_value_name_float(i, "gzXX", GZInfo[i][gCoords][2]);
	    cache_get_value_name_float(i, "gzYY", GZInfo[i][gCoords][3]);
	    cache_get_value_name_int(i, "gzGang", GZInfo[i][gFrakVlad]);
	    if(GZInfo[i][gCoords][0] <= 0)
	    {
			GZInfo[i][gCoords][0] = STANDARTGangZone[i][0];
			GZInfo[i][gCoords][1] = STANDARTGangZone[i][1];
			GZInfo[i][gCoords][2] = STANDARTGangZone[i][2];
			GZInfo[i][gCoords][3] = STANDARTGangZone[i][3];
			format ( string, sizeof ( string ), "INSERT INTO `gangzone` (`gzX`,`gzY`,`gzXX`,`gzYY`) VALUES ('%f','%f','%f','%f')",
			GZInfo[i][gCoords][0], GZInfo[i][gCoords][1], GZInfo[i][gCoords][2], GZInfo[i][gCoords][3]) ;
			mysql_tquery ( connects, string ) ;
	    }
		GZInfo[i][gID] = GangZoneCreate(GZInfo[i][gCoords][0],GZInfo[i][gCoords][1],GZInfo[i][gCoords][2],GZInfo[i][gCoords][3]);
		ZoneOnBattle[i] = 0;
		GZSafeTime[i] = 0;
	    idx++;
	}
	printf("[Chatvirtva]: GangZone Chatvirtulia [%d шт.]", idx);
	return 1;
}
