        if(IsAArm(params[0]) && PlayerInfo[params[0]][pRank] >= 3) { ftext = "Полицию СФ"; ChosenSkin[params[0]] = !t_gen ? 280 : 211; }
        else return err(!"[Shecdoma]: {AFAFAF}Этот игрок не отслужил");
    }
    else if (PlayerInfo[playerid][pLeader] == 21 || PlayerInfo[playerid][pMember] == 21 && PlayerInfo[playerid][pRank] >= 8)
    {
        if(IsAArm(params[0]) && PlayerInfo[params[0]][pRank] >= 3) { ftext = "Полицию ЛВ"; ChosenSkin[params[0]] = !t_gen ? 280 : 76; }
        else return err(!"[Shecdoma]: {AFAFAF}Этот игрок не отслужил");
    }
    else return err(!"[Shecdoma]: {AFAFAF} Вам не доступна данная функция.");

    //leave_team(params[0], PlayerInfo[params[0]][pMember]);

    PlayerInfo[params[0]][pMember] = PlayerInfo[playerid][pMember];

    //invite_team(params[0], PlayerInfo[params[0]][pMember]);

    PlayerInfo[params[0]][pRank] = 1;
    PlayerInfo[params[0]][pJob] = 0;
    PlayerInfo[params[0]][pModel] = ChosenSkin[params[0]];
    SetPlayerSkinEx(params[0], PlayerInfo[params[0]][pModel]);
    format(string_,sizeof(string_),"Вы пригласили %s присоедениться к %s", PlayerInfo[params[0]][pName],ftext);
    SCM(playerid,0x6AB1FFAA,string_);
    format(string_,sizeof(string_),"Вы были приглашены в %s лидером %s", ftext, PlayerInfo[playerid][pName]);
    SCM(params[0],0x6AB1FFAA,string_);
    format(query_, sizeof query_ , "UPDATE `accounts` SET `pMember` = '%d', `pRank` = '%d', `pJob` = '%d', `pModel` = '%d', `pSpawnChange` = '%d' WHERE `Name` = '%s'",
