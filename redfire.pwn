ToggleLock(playerid,vehicleid)
{
    if(veh_info[vehicleid-1][v_locked])
	{
	    if(house_car[playerid] == vehicleid)
	    {
	        gPlayerHOW[playerid][howLock] = 0;
			format(cmdstr,sizeof(cmdstr), "{FFFFFF}Тип: %s\n{FFFFFF}Mflobeli: {33AA33}%s\n{FFFFFF}Дверь: %s (L.ALT)", (!gPlayerHOW[playerid][howType]) ? ("{33AA33}Стандарт") : ("{FFFB00}Улучшенный"), Name(playerid), (!gPlayerHOW[playerid][howLock]) ? ("{33AA33}Открыто") : ("{F04245}Закрыто"));

	    	UpdateDynamic3DTextLabelText(gVehicleHOW[house_car[playerid]][HOWText],0xFFFFFFFF,cmdstr);
	    }
		GameTextForPlayer(playerid, "~g~UNLOCK", 5000, 3);
		UnLockCar(vehicleid);
		PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
		return false;
	}
	else
	{
	    if(house_car[playerid] == vehicleid)
	    {
	        gPlayerHOW[playerid][howLock] = 1;
	        format(cmdstr,sizeof(cmdstr), "{FFFFFF}Тип: %s\n{FFFFFF}Mflobeli: {33AA33}%s\n{FFFFFF}Дверь: %s (L.ALT)", (!gPlayerHOW[playerid][howType]) ? ("{33AA33}Стандарт") : ("{FFFB00}Улучшенный"), Name(playerid), (!gPlayerHOW[playerid][howLock]) ? ("{33AA33}Открыто") : ("{F04245}Закрыто"));

	    	UpdateDynamic3DTextLabelText(gVehicleHOW[house_car[playerid]][HOWText],0xFFFFFFFF,cmdstr);
	    }
		GameTextForPlayer(playerid, "~r~LOCK", 5000, 3);
		LockCar(vehicleid);
		if(!IsPlayerInVehicle(playerid,vehicleid)) SetVehicleParamsCarWindows(vehicleid,1,1,1,1);
		SetVehicleParamsCarDoors(vehicleid,0,0,0,0);
		PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
		return true;
	}
}
