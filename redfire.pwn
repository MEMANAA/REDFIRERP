CMD:buyhouse(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
	if(!IsPlayerConnected(playerid)) return true;
	for(new h = 1; h <= TotalHouse; h++)
	{
		if(PlayerToPoint(2.0, playerid, HouseInfo[h][hEntrancex], HouseInfo[h][hEntrancey], HouseInfo[h][hEntrancez]) && HouseInfo[h][hOwned] == 0)
		{
			if(PlayerInfo[playerid][pPhousekey] != 999 && strcmp(Name(playerid), HouseInfo[PlayerInfo[playerid][pPhousekey]][hOwner], true) == 0) return SCM(playerid, COLOR_GREY, "У Вас уже есть дом!");
			if(PlayerInfo[playerid][pKvartirakey] != 0) return SCM(playerid,COLOR_GREY,"Для начала продайте квартиру!");
			if(PlayerInfo[playerid][pBank] < HouseInfo[h][hValue]) return SCM(playerid,COLOR_GREY,"На Вашем банковском счету нет столько средств!");
			PlayerInfo[playerid][pPhousekey] = h;
			HouseInfo[h][hOwned] = 1;
			HouseInfo[h][hHel] = 0;
			if(HouseInfo[h][hKlass] == 5) HouseInfo[h][hTakings] = 9999; // 0 n 1 d 2 c 3 b 4 a s 5
			else HouseInfo[h][hTakings] = 2;
			strmid(HouseInfo[h][hOwner],Name(playerid), 0, strlen(Name(playerid)), 32);
			PlayerInfo[playerid][pBank] -= HouseInfo[h][hValue];
			UpdateTableLog(PlayerInfo[playerid][pName], "-1", "Покупка дома", HouseInfo[h][hValue]);
			SetPlayerInterior(playerid,HouseInfo[h][hInt]);
			SetPlayerPosEx(playerid,HouseInfo[h][hExitx],HouseInfo[h][hExity],HouseInfo[h][hExitz]);
			PlayerInfo[playerid][pInt] = HouseInfo[h][hInt];
			SetPlayerVirtualWorld(playerid,h+50);
   			SCM(playerid, COLOR_GREEN, "Поздравляем Вас с успешной покупкой дома!");
      		SCM(playerid, COLOR_BLUE, "Внимание!"W" Вас могут выселить если дом не будет вовремя оплачен");
      		SCM(playerid, COLOR_BLUE, "[Minishneba]"W" Shetana домашний счёт или узнать баланс можно в банке");
			GameTextForPlayer(playerid,"~w~welcome home~n~print:~g~/exit", 5000, 3);
			BuyHouse(h);
			PlayerInfo[playerid][pSpawnChange] = 1;
			OnPlayerUpdateAc(playerid);
			SaveHouse(h);
			return true;
		}
	}
	return true;
}
