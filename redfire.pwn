stock KickAC(playerid, code, reason[])
{
	if(PlayerInfo[playerid][pAdmin] >= 1) return true;
	//if(code == 339 && code == 340 && GLS[11] == 339) return 1;
	for(new i; i < 18; i++)
	{
		if(GLS[i] == code) return 1;
	}
	if(!NeedToKick[playerid])
	{
	    DelGun(playerid);
	    printf("PLAYER %s KICKED IN AC %d", PlayerInfo[playerid][pName], code);
		format(cmdstr, sizeof(cmdstr), "[Anti-Cheat]: кикнут: %s, код ошибки: %d (%s)", PlayerInfo[playerid][pName], code, reason);
	    ABroadCast(COLOR_LIGHTRED,cmdstr,1);
	    WriteLog("KickCheat.txt", cmdstr);
		SCMF(playerid,COLOR_REDD," {FF0000}Вы были кикнуты за использования постороннего ПО {FFFFFF} (%s %d | Pc.Loss: %.2f)", reason, code, NetStats_PacketLossPercent(playerid));
		SendClientMessage(playerid,-1,"Если произошла Shecdoma, отпишитесь на форуме (тех. раздел): "NameForum"");
	}
	if(code == 8 || code == 9) DelGun(playerid);
	NeedToKick[playerid] = 1;
	//if(code == 313) BlockIpAddress(PlayerIp[playerid], 120 * 1000); // заблокирует IP-адрес на минуту
	return Kick(playerid);
}
