publics:callback_offmembers(playerid)
{
	new rows;
	cache_get_row_count(rows);
	if(! rows)
	{
		SendClientMessage(playerid, col_gray, "Am Parametris Mkone Motamashe Ver Moidzebna.");
		ShowLeaderMenu(playerid);
		DeletePVar(playerid, "ofm_list_page");
		return 1;
	}
	new rows_list = GetPVarInt(playerid, "ofm_list_page")- 1;
	SetPVarInt(playerid, "ofm_list_rows", rows);
	new line_string[MAX_PLAYER_NAME + 37],
		pl_ofm_name[20],
		pl_ofm_rank,
		pl_ofm_online[16];

	new dialog_string[10 * sizeof(line_string)+ 50];
	strcat(dialog_string, ""c_server"Wina Gverdi\n"c_server"Shemdegi Gverdi\n");
	for(new i = rows_list * 10; i <  rows_list * 10 + 10; i ++)
	{
		if(i >= rows)break;

		cache_get_value_name(i, "Name", pl_ofm_name, 20);
		cache_get_value_name_int(i, "pRank", pl_ofm_rank);
		cache_get_value_name(i, "pOnline", pl_ofm_online, 16);

		new pvar_string[8];
		format(pvar_string, sizeof(pvar_string), "ofm_%d", i - rows_list * 10);
		SetPVarString(playerid, pvar_string, pl_ofm_name);

		format(line_string, sizeof(line_string), "%s"c_server"\t%s(%d){ffffff}\t%s\n", pl_ofm_name, f_rank[PlayerInfo[playerid][pMember] - 1][pl_ofm_rank - 1], pl_ofm_rank, pl_ofm_online);
		strcat(dialog_string, line_string);

	}

	ShowPlayerDialogEx(playerid, d_offmembers_list, DIALOG_STYLE_TABLIST, ""c_server"Organizaciis Wverebi Offline", dialog_string, "Datanxmeba", "Ukan");
