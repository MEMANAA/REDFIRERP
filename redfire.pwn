		if ( veh_info [ vehicleid - 1 ] [ v_int ] != 0 )LinkVehicleToInterior ( vehicleid, veh_info [ vehicleid - 1 ] [ v_int ] ) ;
		if ( veh_info [ vehicleid - 1 ] [ v_vw ] != 0 )SetVehicleVirtualWorld ( vehicleid, veh_info [ vehicleid - 1 ] [ v_vw ] ) ;
	}
	if ( veh_info [ vehicleid - 1 ] [ v_cargo_object ] [ 0 ] != 0 )
	{
		for ( new j = 0 ; j < 2 ; j ++ )
		{
			DestroyDynamicObject ( veh_info [ vehicleid - 1 ] [ v_cargo_object ] [ j ] ) ;
			veh_info [ vehicleid - 1 ] [ v_cargo_object ] [ j ] = 0 ;
		}
	}
	if ( veh_info [ vehicleid - 1 ] [ v_type ] == vehicle_type_player && veh_info [ vehicleid - 1 ] [ v_sell_price ] == 0 && ! IsAPlane ( vehicleid ) )
	{
		GetNoParking(vehicleid);
		for ( new j = 0; j < 10; j ++ )
		{
			if ( veh_info [ vehicleid - 1 ] [ v_component ] [ j ] == 0 ) continue ;
			AddVehicleComponent ( veh_info [ vehicleid - 1 ] [ v_vehicle ], veh_info [ vehicleid - 1 ] [ v_component ] [ j ] ) ;
		}
	}
	if ( veh_info [ vehicleid - 1 ] [ v_fine ] > 0 ) veh_info [ vehicleid - 1 ] [ v_fine ] = 0 ;
    if ( veh_info [ vehicleid - 1 ] [ v_type ] == vehicle_type_server ) veh_info [ vehicleid - 1 ] [ v_fuel ] = 300.0 ;
    if(vehicleid >= TruckInfo[VEHICLES][0] && vehicleid <= TruckInfo[VEHICLES][32])
	{
	    UnLockCar(vehicleid);
	    veh_info [ vehicleid - 1 ] [ v_fuel ] = 200;
	    foreach(new i:Player)
	    {
	        if(GetPVarInt(i, "get_truck_id") == vehicleid) DeletePVar(i, "get_truck_id");
	    }
	}
    ServerHPVehicle[vehicleid] = 1000.0;
	zavodis[vehicleid] = 0;
	enginebrokened[vehicleid] = 0;
	foreach(new i: Player)
	{
		if(vehicleid == PlayerInfo[i][TaxiCarID]) cmd::fare_s(i);
		if(vehicleid == robberycar[i]) leave_robhouse(i);
	    if(GetVehicleModel(vehicleid) == 462)
		{
			if(rfaggio[i] == vehicleid)
			{
	 			DestroyVehicle(vehicleid);
	 			rfaggio[i] = INVALID_VEHICLE_ID;
	 			SCM(i,COLOR_GREY, "Ваш арендованный транспорт был отбуксирован!");
			}
		}
	}
	if(vehcreat[vehicleid] == 1)
	{
		vehcreat[vehicleid] = 0;
		DestroyVehicle(vehicleid);
	}
	if(TaxiCarr)
	{
		GetVehicleParamsEx(TaxiCarr,engine,lights,alarm,doors,bonnet,boot,objective);
 		SetVehicleParamsEx(TaxiCarr,engine,lights,alarm,doors,bonnet,true,objective);
 		LockCar(TaxiCarr);
 		AddVehicleComponent(TaxiCarr,1080);
 	}
 	if(NarkCar)
	{
		GetVehicleParamsEx(NarkCar,engine,lights,alarm,doors,bonnet,boot,objective);
 		SetVehicleParamsEx(NarkCar,engine,lights,alarm,doors,bonnet,true,objective);
 		LockCar(NarkCar);
 		AddVehicleComponent(NarkCar,1080);
 	}
 	if(vehicleid >= rentcarls[0] && vehicleid <= rentcarls[8])
	{
	    UnLockCar(vehicleid);
	    veh_info [ vehicleid - 1 ] [ v_fuel ] = 200;
	}
	else if(vehicleid >= rentcarvip[0] && vehicleid <= rentcarvip[8])
	{
	    UnLockCar(vehicleid);
	    veh_info [ vehicleid - 1 ] [ v_fuel ] = 200;
	}
	else if(vehicleid >= rentcarsf[0] && vehicleid <= rentcarsf[5])
	{
	    UnLockCar(vehicleid);
	    veh_info [ vehicleid - 1 ] [ v_fuel ] = 200;
	}
	else if(vehicleid >= rentcarlv[0] && vehicleid <= rentcarlv[17])
	{
	    UnLockCar(vehicleid);
	    veh_info [ vehicleid - 1 ] [ v_fuel ] = 200;
	}
	else if(vehicleid >= liccar[0] && vehicleid <= liccar[12])
	{
		UnLockCar(vehicleid);
	    veh_info [ vehicleid - 1 ] [ v_fuel ] = 200;
	}	
	else if(vehicleid >= fbicar[0] && vehicleid <= fbicar[30])
	{
		if(vehicleid == fbicar[29] || vehicleid == fbicar[30]) SetVehicleVirtualWorld(vehicleid, 0);
		else SetVehicleVirtualWorld(vehicleid, 2);
	}	
	if(gVehicleGun[vehicleid][vGunAmmo]) gVehicleGun[vehicleid][vGunAmmo] = 0;
	if(VG[vehicleid][vgDrugs])
	{
		SendGhettoMessage(COLOR_RED, "ВНИМАНИЕ! Партия наркотиков уничтожена");  
		SendCopsMessage(COLOR_GREEN2, "ВНИМАНИЕ! Партия наркотиков уничтожена, хорошая работа!");
		VG[vehicleid][vgDrugs] = 0;
		TimeToDrug = 1;
	}
	if(VG[vehicleid][vgMats]) VG[vehicleid][vgMats] = 0;
 	return true;
}
