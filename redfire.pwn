    if(!IsAMafia(playerid)) return true;
	if(PlayerInfo[playerid][pRank] < 3) return SCM(playerid, COLOR_GREY, "Вы должны быть 3 рангом");
	if(sscanf(params, "u",params[0])) return SCM(playerid, COLOR_WHITE, "Введите: /takephone [id]");
	if(!IsPlayerConnected(params[0])) return true;
	if(!ProxDetectorS(8.0, playerid, params[0])) return true;
	if(!GetPVarInt(params[0], #PHONE_STATUS) && PlayerTied[params[0]])
	{
		SetPVarInt(params[0], #PHONE_STATUS, 1);
		
		SCMF(params[0], -1, " %s забрал у Вас телефон", Name(playerid));
		SCMF(playerid, -1, " Вы забрали телефон у %s", Name(params[0]));
	}
	else if(GetPVarInt(params[0], #PHONE_STATUS))
	{
		SCMF(params[0], -1, " %s вернул Вам телефон", Name(playerid));
		SCMF(playerid, -1, " Вы вернули телефон %s", Name(params[0]));
		DeletePVar(playerid, #PHONE_STATUS);
	}	
	return true;
}
