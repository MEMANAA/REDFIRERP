CMD:accept(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
	new giveplayer[MAX_PLAYER_NAME];
	new x_job[10];
	if(sscanf(params, "s[10]",x_job)) return SCM(playerid, COLOR_WHITE, "Введите: /accept [значение]");
	if(strcmp(x_job,"gun",true) == 0)
	{
	    if(PlayerInfo[sellgun4[playerid]][pMats] < (2*sellgun2[playerid]))
		{
		    SCM(sellgun4[playerid],COLOR_RED, "У вас недостаточно матов");
		    sellgun1[playerid] = 0; sellgun2[playerid] = 0; sellgun3[playerid] = 0; sellgun4[playerid] = 0;
			SetPVarInt(playerid, "otvet", 0);
			return SCM(playerid, COLOR_RED, "У продавца нет материалов");
		}
	    if(sellgun1[playerid] == 0) return SCM(playerid,COLOR_GREY,"Вам не предлагали Yidva оружие!");
	    if(PlayerInfo[playerid][pCash] < sellgun3[playerid]) return SCM(playerid,COLOR_GRAD1, "У вас Ar Aris Sakmarisi Tanxa");
		PlayerInfo[playerid][pCash] -= sellgun3[playerid];
		give_weapon(playerid, sellgun1[playerid], sellgun2[playerid]);
		PlayerInfo[sellgun4[playerid]][pMats] -= (2*sellgun2[playerid]);
		PlayerInfo[sellgun4[playerid]][pCash] += sellgun3[playerid];
		format(cmdstr, sizeof(cmdstr), "%s сделал оружие из материалов %s",Name(sellgun4[playerid]),Name(playerid));
		SendStreamMessage(10.0, playerid, cmdstr, COLOR_PURPLE);
		sellgun1[playerid] = 0; sellgun2[playerid] = 0; sellgun3[playerid] = 0; sellgun4[playerid] = 0;
		SetPVarInt(playerid, "otvet", 0);
		return true;
	}
