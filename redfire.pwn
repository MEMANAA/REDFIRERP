stock isNumeric(const stringa[])
{
  new length=strlen(stringa);
  if (length<=0) return false;
  for (new i = 0; i < length; i++)
    {
      if (
            (stringa[i] > '9' || stringa[i] < '0' && stringa[i]!='-' && stringa[i]!='+') // Not a number,'+' or '-'
             || (stringa[i]=='-' && i!=0)                                             // A '-' but not at first.
             || (stringa[i]=='+' && i!=0)                                             // A '+' but not at first.
         ) return false;
    }
  if (length==1 && (stringa[0]=='-' || stringa[0]=='+')) return false;
  return true;
}
