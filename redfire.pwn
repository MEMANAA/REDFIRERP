CMD:bizlock(playerid, params[])
{
	if(IsPlayerConnected(playerid))
	{
		for(new i = 0; i < sizeof(BizzInfo); i++)
		{
			if(PlayerToPoint(5.0, playerid,BizzInfo[i][bEntranceX], BizzInfo[i][bEntranceY], BizzInfo[i][bEntranceZ])
		    || PlayerToPoint(5.0, playerid,BizzInfo[i][bBarX], BizzInfo[i][bBarY], BizzInfo[i][bBarZ]) && i > 40)
		    {
				if((PlayerInfo[playerid][pPbiskey] == i) || (PlayerInfo[playerid][pMember] == 2) && (PlayerInfo[playerid][pRank] >= 5) || PlayerInfo[playerid][pAdmin] >= 5)
				//if(BizzInfo[i][bLicense] == 1) return SCM(playerid, COLOR_RED, "У этого бизнеса есть лицензия!");
				{
					if(BizzInfo[i][bLocked] == 1)
					{
						if(FI[7][fMoney] <= 0) return SCM(playerid, COLOR_GREY, "Казна опусташена!");
						BizzInfo[i][bLocked] = 0;
						SendClientMessage(playerid, COLOR_WHITE, "Вы открыли бизнес");
						PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
					}
					else if(BizzInfo[i][bLocked] == 0)
					{
						BizzInfo[i][bLocked] = 1;
						SendClientMessage(playerid, COLOR_WHITE, "Вы закрыли бизнес");
						PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
					}
				}
			}
		}
	}
	return true;
}
