CMD:pay(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(sscanf(params, "ud", params[0],params[1])) return	SCM(playerid, COLOR_WHITE, "Введите: /pay [id] [сумма]");
    if(!IsPlayerConnected(params[0])) return true;
	if(params[0] == INVALID_PLAYER_ID) return true;
	if(playerinduel[playerid]) return err(!"[Shecdoma]: {AFAFAF}Передавать деньги во время дуэли запрещено!");
	if(playerinduel[params[0]]) return err(!"[Shecdoma]: {AFAFAF}Передавать деньги во время дуэли запрещено!");
	if(params[0] == playerid) return SCM(playerid, COLOR_WHITE, "Вы не можете передать деньги самому себе!");
	if(ZapretDice[params[0]] == 1) return SCM(playerid,-1,"В данный момент у вас активная ставка в казино!");
    if((params[1] > 1000 && PlayerInfo[playerid][pLevel] < 2) || (params[1] > 1000 && PlayerInfo[playerid][pLevel] < 3)) return SendClientMessage(playerid, COLOR_GRAD1, "Вы должны быть 3 лвл, чтобы передать больше 1000 $!");
    if(PlayerInfo[playerid][pLevel] == 1) return SendClientMessage(playerid, COLOR_GREY, "Команда доступна только с 2 уровня");
    if(GetPlayerState(params[0]) == PLAYER_STATE_SPECTATING) return SCM(playerid, COLOR_GRAD1, "Вы слишком далеко.");
	if(params[1] < 1 || params[1] > 100000) return SCM(playerid, COLOR_GRAD1, "Нельзя передать меньше 1 $ и больше 99999 $");
	if(!ProxDetectorS(5.0, playerid, params[0])) return SCM(playerid, COLOR_GRAD1, "Вы слишком далеко.");
	if(GetSRVMoney(playerid) < params[1]) return true;
	if(GetString(PlayerIp[playerid],PlayerIp[params[0]] ))
	{
		SendClientMessage(playerid, COLOR_RED, "Вы не можете передать деньги в связи с причинами указаными ниже:");
		SendClientMessage(playerid, COLOR_RED, "Вы подозреваетесь в мошенничестве, все данные были отосланы администраторам");
		SendClientMessage(playerid, -1, "Вы можете обжаловать решение античита на форуме redfire-rp.ge/forum");
		format(cmdstr, sizeof(cmdstr), "%s[%d][%s] И %s[%d][%s] ПОДОЗРЕВАЮТСЯ В ПОПЫТКЕ ФАРМА ВИРТОВ.",Name(playerid),playerid,PlayerIp[playerid],Name(params[0]),params[0],PlayerIp[params[0]]);
		SendAdminMessage(COLOR_RED,cmdstr);
		WriteLog("FarmLog.txt", cmdstr);
		return true;
	}
	GiveMoney(params[0], params[1], "/pay");
	GiveMoney(playerid, -params[1], "/pay");
	format(cmdstr, sizeof(cmdstr), "Вы передали %s[%d], %d $.",Name(params[0]),params[0],params[1]);
	PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
	SCM(playerid, COLOR_GRAD1, cmdstr);
	format(cmdstr, sizeof(cmdstr), "Вы получили %d $ от %s[%d].",params[1],Name(playerid), playerid);
	SCM(params[0], COLOR_GRAD1, cmdstr);
	PlayerPlaySound(params[0], 1052, 0.0, 0.0, 0.0);
	format(cmdstr,sizeof(cmdstr), "достал бумажник и передал деньги %s",Name(params[0]));
	SetPlayerChatBubble(playerid,cmdstr,COLOR_PURPLE,30.0,10000);
	return true;
}
