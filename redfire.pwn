			SetPlayerPosEx(playerid, 1593.22, -1136.10, 170.10);
			SetPlayerInterior(playerid, 0);
			SetPlayerVirtualWorld(playerid, 0);
			setFreezePlayerForTime(playerid, 2);
			SendClientMessage(playerid, COLOR_YELLOW, "[Minishneba] "W"Вы зарегистрировались в Мероприятии 'Падающие плиты'");
		}
	}
	mp_fall[playerid] = true;
	DelGun(playerid);

    return 1;
