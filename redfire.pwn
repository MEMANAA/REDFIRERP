CMD:setgps ( playerid )
{
	if ( PlayerInfo [ playerid ] [ gps_tracker ] <= 0 ) return SendClientMessage ( playerid, col_gray, " У Вас нет GPS-трекера." ) ;
	new veh_id = GetPlayerVehicleID ( playerid ) ;
	if ( veh_id == 0 ) return SendClientMessage ( playerid, col_gray, " Вы должны быть в транспорте." ) ;
	if ( veh_info [ veh_id - 1 ] [ v_type ] != vehicle_type_player )return SendClientMessage ( playerid, col_gray, " GPS-трекер можно установить только на личный транспорт." ) ;
	if ( veh_info [ veh_id - 1 ] [ v_gps_tracker ] == 1  )return SendClientMessage ( playerid, col_gray, " GPS-трекер уже установлен." ) ;

	veh_info [ veh_id - 1 ] [ v_gps_tracker ] = 1 ;
	PlayerInfo [ playerid ] [ gps_tracker ] -= 1 ;
	UpdatePlayerData(playerid, "gps_tracker", PlayerInfo[playerid][gps_tracker]);
	//update_int_sql ( playerid, "u_gps_tracker", PlayerInfo [ playerid ] [ gps_tracker ] ) ;

	new scm_string [ 98 ] ;
	format ( scm_string, 98, "UPDATE `users_vehicles` SET `v_gps_tracker` = '%d' WHERE `v_id` = '%d'",
	veh_info [ veh_id - 1 ] [ v_gps_tracker ],
	veh_info [ veh_id - 1 ] [ v_id ] ) ;
	mysql_tquery ( connects, scm_string ) ;

	SendClientMessage ( playerid, -1, "{"#cGN"}* {"#cWH"}GPS-трекер успешно установлен в транспорт." ) ;
	return 1 ;
}
