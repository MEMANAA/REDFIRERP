	SendClientMessage(playerid, COLOR_YELLOW, stringa);
	string[0] = EOS;
	mysql_format(connects, string,500,"INSERT INTO `reports`(`rNick`,`gameid`,`rDate`,`rText`) VALUES ('%s','%d','%02d:%02d:%02d','%s')",PlayerInfo[playerid][pName],playerid,h,m,s,stringa);
	new report_id, Cache:insert_result = mysql_query(connects, string);
	report_id = cache_insert_id();
	if (cache_is_valid(insert_result)) cache_delete(insert_result);
	new Cache:result = mysql_query(connects, "SELECT id FROM `reports`"), rows;
	rows = cache_num_rows();
	string[0] = EOS;
	if(rows > 3) format(string, 128, " Жалоба от %s[%d] на %s[%d]: {FFFFFF}%s "P"[/ti] {FF0000}Уже %d репортов!!!", PlayerInfo[playerid][pName], playerid, PlayerInfo[params[0]][pName], params[0], params[1], rows);
	else format(string, 128, " Жалоба от %s[%d] на %s[%d]: {FFFFFF}%s "P"[/ti]", PlayerInfo[playerid][pName], playerid, PlayerInfo[params[0]][pName], params[0], params[1]);
	SendAdminMessage(0x2A9170FF, string);
	SCMF(playerid, COLOR_YELLOW2, " Ваш запрос #%i успешно отправлен администрации. Вы %i в очереди!", report_id, rows);
	if (cache_is_valid(result)) cache_delete(result);
	strmid(PlayerInfo[playerid][pReportMessage], params[1], 0, strlen(params[1]), strlen(params[1])+5);
	SetPVarInt(playerid,"AntiReport",60);
	if(PlayerInfo[playerid][pDonateRank] > 1) SetPVarInt(playerid,"AntiReport",30);
