	cache_get_row_count(rows);
	if(!rows) return SCM(i, COLOR_WHITE, "Аккаунт не найден!");
	cache_get_value_name(0, "pIpReg", maximum), strmid(preg, maximum, 0, strlen(maximum), 16);
	cache_get_value_name(0, "pIp", maximum), strmid(pip, maximum, 0, strlen(maximum), 16);
