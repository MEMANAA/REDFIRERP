CMD:park ( playerid )
{
	if ( GetPVarInt ( playerid, "freeze_status" ) )return SendClientMessage ( playerid, col_gray, "Вы не можете припарковать автомобиль в данный момент." ) ;
	if ( GetPlayerState ( playerid ) != PLAYER_STATE_DRIVER )return SendClientMessage ( playerid, col_gray, "Вы должны находиться за рулём транспортного средства." ) ;
	new veh_id = GetPlayerVehicleID ( playerid ) ;
	if ( IsValidVehicle ( veh_id ) )
	{
		if ( veh_info [ veh_id - 1 ] [ v_owner ] != PlayerIDs [ playerid ] && veh_info [ veh_id - 1 ] [ v_type ] != vehicle_type_player && house_car[playerid] != veh_id)return SendClientMessage(playerid, col_gray,"Вы должны находиться в Вашем личном транспорте!");
		new Float:vehicle_health ;
		GetVehicleHealth ( veh_id, vehicle_health ) ;
		if ( vehicle_health < 950 ) return SendClientMessage ( playerid, col_gray, "Для начала нужно починить транспортное средство." ) ;
		if(GetVehicleSpeed_(house_car[playerid]) > 1) return SendClientMessage(playerid, col_gray,!" Нельзя парковаться на высокой скорости!");
		for ( new j = 1 ; j < MAX_VEHICLES ; j ++ )
		{
			if ( ! IsValidVehicle ( j ) ) continue;
			if ( veh_info [ j - 1 ] [ v_vw ] == GetPlayerVirtualWorld ( playerid ) && IsPlayerInRangeOfPoint ( playerid, 3.0, veh_info [ j - 1 ] [ v_pos ] [ 0 ], veh_info [ j - 1 ] [ v_pos ] [ 1 ], veh_info [ j - 1 ] [ v_pos ] [ 2 ] ) ) return SendClientMessage ( playerid, col_gray, "Невозможно припарковать транспорт вбилизи парковки другого транспортного средства." ) ;
		}
		new Float:X,
		    Float:Y,
		    Float:Z;
		GetVehiclePos(veh_id, X, Y, Z);
		if(Z > 40) return SendClientMessage(playerid,COLOR_GREY,!" Запрещено парковаться высоко от земли!");
		if(veh_id == house_car[playerid])
		{
  			if(IsVehicleInRangeOfPoint(house_car[playerid], 120, 2754.5813,-2460.4622,13.6432) || IsVehicleInRangeOfPoint(house_car[playerid], 100, 1480.0197,-1693.0457,14.0467)
			|| IsVehicleInRangeOfPoint(house_car[playerid], 50, 1688.4587,762.0396,10.8203)
			|| IsVehicleInRangeOfPoint(house_car[playerid], 50, -24.4761,-179.5872,1008.6170)) return SendClientMessage(playerid,COLOR_RED,!" В этом месте запрещено парковаться!");
			if(IsVehicleInRangeOfPoint(house_car[playerid], 150, HOW[0],HOW[1],HOW[2])
			|| IsVehicleInRangeOfPoint(house_car[playerid], 150, HOW2[0],HOW2[1],HOW2[2])) return SendClientMessage(playerid,COLOR_RED,!" Парковка разрешена за два квадрата от захвата территории");
		}
		GetVehiclePos ( veh_id, veh_info [ veh_id - 1 ] [ v_pos ] [ 0 ], veh_info [ veh_id - 1 ] [ v_pos ] [ 1 ], veh_info [ veh_id - 1 ] [ v_pos ] [ 2 ] ) ;
		GetVehicleZAngle ( veh_id, veh_info [ veh_id - 1 ] [ v_pos ] [ 3 ] ) ;
		if(veh_id == house_car[playerid])
		{
  			gPlayerHOW[playerid][howPosX] = veh_info [ veh_id - 1 ] [ v_pos ] [ 0 ];
			gPlayerHOW[playerid][howPosY] = veh_info [ veh_id - 1 ] [ v_pos ] [ 1 ];
			gPlayerHOW[playerid][howPosZ] = veh_info [ veh_id - 1 ] [ v_pos ] [ 2 ];
			gPlayerHOW[playerid][howPosC] = veh_info [ veh_id - 1 ] [ v_pos ] [ 3 ];
			HouseOnWheelsSave(playerid);
		}
		veh_info [ veh_id - 1 ] [ v_type ] = vehicle_type_player ;
		veh_info [ veh_id - 1 ] [ v_id ] = veh_info [ veh_id - 1 ] [ v_id ] ;
		veh_info [ veh_id - 1 ] [ v_pos ] [ 0 ] = veh_info [ veh_id - 1 ] [ v_pos ] [ 0 ] ;
		veh_info [ veh_id - 1 ] [ v_pos ] [ 1 ] = veh_info [ veh_id - 1 ] [ v_pos ] [ 1 ] ;
		veh_info [ veh_id - 1 ] [ v_pos ] [ 2 ] = veh_info [ veh_id - 1 ] [ v_pos ] [ 2 ] ;
		veh_info [ veh_id - 1 ] [ v_pos ] [ 3 ] = veh_info [ veh_id - 1 ] [ v_pos ] [ 3 ] ;
		veh_info [ veh_id - 1 ] [ v_owner ] = veh_info [ veh_id - 1 ] [ v_owner ] ;
		veh_info [ veh_id - 1 ] [ v_color ] [ 0 ] = veh_info [ veh_id - 1 ] [ v_color ] [ 0 ] ;
		veh_info [ veh_id - 1 ] [ v_color ] [ 1 ] = veh_info [ veh_id - 1 ] [ v_color ] [ 1 ] ;
		veh_info [ veh_id - 1 ] [ v_paint ] = veh_info [ veh_id - 1 ] [ v_paint ] ;
		veh_info [ veh_id - 1 ] [ v_supreme ] = veh_info [ veh_id - 1 ] [ v_supreme ] ;
		if ( veh_info [ veh_id - 1 ] [ v_paint ] != 3 ) ChangeVehiclePaintjob ( veh_id, veh_info [ veh_id - 1 ] [ v_paint ] ) ;
		veh_info [ veh_id - 1 ] [ v_locked ] = veh_info [ veh_id - 1 ] [ v_locked ] ;
		veh_info [ veh_id - 1 ] [ v_plate ] = veh_info [ veh_id - 1 ] [ v_plate ] ;
		format ( veh_info [ veh_id - 1 ] [ v_plate ], 12, "%s", veh_info [ veh_id - 1 ] [ v_plate ] ) ;
		veh_info [ veh_id - 1 ] [ v_fuel ] = veh_info [ veh_id - 1 ] [ v_fuel ] ;
		veh_info [ veh_id - 1 ] [ v_millage ] = veh_info [ veh_id - 1 ] [ v_millage ] ;
		veh_info [ veh_id - 1 ] [ v_engine_boost ] = veh_info [ veh_id - 1 ] [ v_engine_boost ] ;
		veh_info [ veh_id - 1 ] [ v_brake_boost ] = veh_info [ veh_id - 1 ] [ v_brake_boost ] ;
		veh_info [ veh_id - 1 ] [ v_stability_boost ] = veh_info [ veh_id - 1 ] [ v_stability_boost ] ;
		veh_info [ veh_id - 1 ] [ v_vehicle ] = veh_id ;
		veh_info [ veh_id - 1 ] [ v_vw ] = GetPlayerVirtualWorld ( playerid ) ;
		veh_info [ veh_id - 1 ] [ v_int ] = GetPlayerInterior ( playerid ) ;
		if ( veh_info [ veh_id - 1 ] [ v_int ] != 0 )LinkVehicleToInterior ( veh_id, veh_info [ veh_id - 1 ] [ v_int ] ) ;
		if ( veh_info [ veh_id - 1 ] [ v_vw ] != 0 )SetVehicleVirtualWorld ( veh_id, veh_info [ veh_id - 1 ] [ v_vw ] ) ;
		SetVehicleNumberPlate ( veh_id, veh_info [ veh_id - 1 ] [ v_plate ] ) ;
		veh_info[veh_id - 1][v_stability_boost] = 1;
		new engine, lights, alarm, doors, bonnet, boot, objective ;
		GetVehicleParamsEx ( veh_id, engine, lights, alarm, doors, bonnet, boot, objective ) ;
		SetVehicleParamsEx ( veh_id, engine, lights, alarm, veh_info [ veh_id - 1 ] [ v_locked ], bonnet, boot, objective ) ;
		new scm_string [ 198 ] ;
		SendClientMessage ( playerid, COLOR_GREEN2, "Транспорт успешно припаркован!" ) ;
		if ( veh_info [ veh_id - 1 ] [ v_fine ] > 0 )veh_info [ veh_id - 1 ] [ v_fine ] = 0 ;
		format ( scm_string, 198, "UPDATE `users_vehicles` SET `v_pos_x` = '%.2f', `v_pos_y` = '%.2f', `v_pos_z` = '%.2f',`v_pos_a` = '%.2f',`v_vw` = '%d',`v_int` = '%d' WHERE `v_id` = '%d'",
		veh_info [ veh_id - 1 ] [ v_pos ] [ 0 ], veh_info [ veh_id - 1 ] [ v_pos ] [ 1 ], veh_info [ veh_id - 1 ] [ v_pos ] [ 2 ], veh_info [ veh_id - 1 ] [ v_pos ] [ 3 ],
		veh_info [ veh_id - 1 ] [ v_vw ], veh_info [ veh_id - 1 ] [ v_int ],
		veh_info [ veh_id - 1 ] [ v_id ] ) ;
		mysql_tquery ( connects, scm_string ) ;
	}
	return 1 ;
}
