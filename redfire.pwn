CMD:ticket(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
	if(!IsACop(playerid)) return SCM(playerid, COLOR_GREY, "Вы не полицейский!");
	if(sscanf(params, "uds[54]",params[0],params[1],params[2])) return	SCM(playerid, COLOR_WHITE, "Введите: /ticket [id] [цена] [причина]");
	if(!GetPVarInt(playerid,"Fraction_Duty")) return SCM(playerid, COLOR_GREY, "Вам нужно быть в рабочей форме!");
	if(params[1] < 1 || params[1] > 15000) { SCM(playerid, COLOR_GREY, "Штраф не должен привышать 15000 и не должен быть меньше 0 $!"); return true; }
	if(!IsPlayerConnected(params[0])) return true;
	if(!ProxDetectorS(8.0, playerid, params[0])) return true;
	if(GetSRVMoney(params[0]) < params[1]) 	return 	SCM(playerid, COLOR_GRAD1, "У этого  нет столько денег!");
	if(params[0] == playerid) { SCM(playerid, COLOR_GREY, "[Shecdoma] Вы указали свой ID"); return true; }
	format(cmdstr, sizeof(cmdstr), "Вы выписали штраф в размере %d $ %s. Причина: %s",params[1],Name(params[0]),params[2]);
	SCM(playerid, COLOR_BLUE, cmdstr);
	format(cmdstr, sizeof(cmdstr), "Офицер %s выписал вам штраф в размере %d $. Причина: %s",Name(playerid),params[1],params[2]);
	SCM(params[0], COLOR_BLUE, cmdstr);
	SCM(params[0], COLOR_WHITE, "(( Введите: /accept ticket, чтобы оплатить штраф ))");
	TicketOffer[params[0]] = playerid;
	TicketMoney[params[0]] = params[1];
	return true;
}
