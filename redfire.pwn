publics:cmd_moneylog_callback ( playerid, pl_name [ ] )
{
	new rows;
	cache_get_row_count(rows);
	if ( ! rows ) return SendClientMessage ( playerid, col_gray,"Не найденно данных по передаче средств." ) ;
	string [ 0 ] = EOS ;
	for ( new j = 0 ; j < rows ; j ++ )
	{
		new log_date [ 24 ], log_reason [ 48 ]/*, log_from [ 20 ]*/, log_money, line_string [ 108 ] ;

		//log_money = cache_get_field_content_int ( j, "Amount", connects ) ;
        cache_get_value_name_int(j, "Amount",log_money);
		//cache_get_field_content ( j, "from_name", log_from, connects, 20 ) ;
		cache_get_value_name ( j, "Operation", log_reason) ;
		cache_get_value_name ( j, "DATE", log_date) ;

		format ( line_string, sizeof ( line_string ), "{"#cWH"}%s\t%d$\t%s\t%s\n",pl_name, log_money, log_reason, log_date ) ;
		strcat ( string, line_string ) ;
	}
	ShowPlayerDialogEx ( playerid, 0, DIALOG_STYLE_MSGBOX, "{"#cBLUES"}История финансов игрока", string, "Daxurva", "" ) ;
	return 1 ;
}
