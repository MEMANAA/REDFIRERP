publics: priz_callback(playerid)
{
	new rows;
	cache_get_row_count(rows);
	if(!rows) return SendClientMessage(playerid, COLOR_GREY, "У Вас нет доступных призов! Зайдите на сайт: "W"redfire-rp.ge >> Roullete");
	new priz_number, priz_id, priz_vaule;
	cache_get_value_index_int(0, 0, priz_number);
	cache_get_value_index_int(0, 3, priz_vaule);
    cache_get_value_index_int(0, 4, priz_id);
    rows -=1;
	if(priz_id == 1) // cash
	{
	    GiveMoney ( playerid, priz_vaule, "Приз с рулетки" ) ;
	    format(cmdstr, sizeof(cmdstr), "DELETE FROM `ucp_drop_roulette` WHERE `p_number` = '%d'", priz_number);
