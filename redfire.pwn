    if(sscanf(params, "s[128]",params[0])) return SendEsp(playerid, "/rb [Текст]");
	if ( PlayerInfo [ playerid ] [ f_mute ] )
	{
		new scm_string [ 78 ] ;
		format ( scm_string, sizeof ( scm_string ),"У Вас затычка в чате фракции "W"(%d секунд(ы))", PlayerInfo [ playerid ] [ f_mute ] ) ;
		SendClientMessage ( playerid, col_gray, scm_string ) ;
		return false ;
	}
	if(PlayerInfo[playerid][pMuted] == 1) return SCM(playerid, col_gray, "Ваш чат заблокирован!");
	if(!GetPVarInt(playerid,"Fraction_Duty")) return true;
	else if(IsAIP(params[0])) return 0;
	else if(CheckText(params[0], 2)) return PlayerGetsChatBan(playerid, params[0]);
	if(PlayerInfo[playerid][pMember] == 11 || PlayerInfo[playerid][pLeader] == 11)
	if(duty[playerid][0] == 0) return SCM(playerid, COLOR_GREY, "Необходимо начать рабочий день!");
	switch(PlayerInfo[playerid][pMember])
	{
	    case 1,2,3,4,7,9,10,11,16,19,20,21:
	    {
	        if(GetString(f_rank[PlayerInfo[playerid][pMember] - 1][PlayerInfo[playerid][pRank] - 1], "None")) format(cmdstr, sizeof(cmdstr), "[R] %s %s[%d]: (( %s ))", GetRank(playerid), Name(playerid),playerid, params[0]);
			else format(cmdstr, sizeof(cmdstr), "[R] %s %s[%d]: (( %s ))", f_rank[PlayerInfo[playerid][pMember] - 1][PlayerInfo[playerid][pRank] - 1], Name(playerid), playerid, params[0]);
			SendFamilyMessageOOC(PlayerInfo[playerid][pMember], TEAM_BLUE_COLOR, cmdstr);
	    }
	    default: return 1;
	}
	return true;
}
CMD:db(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
	if(sscanf(params, "s[128]",params[0])) return SCM(playerid, COLOR_WHITE, "Введите: /db [текст] (OOC депортамент)");
	if(PlayerInfo[playerid][pMuted] == 1) return SCM(playerid, COLOR_LIGHTRED, "У вас бан чата!");
	else if(IsAIP(params[0])) return 0;
	else if(CheckText(params[0], 2)) return PlayerGetsChatBan(playerid, params[0]);
	if(PlayerInfo[playerid][pRank] < 2) return SCM(playerid, COLOR_GRAD1, "Вы не уполномочены писать в депортамент!");
	if ((!dchat)) return SCM(playerid, COLOR_GRAD2, "Сообщение можно отправить через 10 секунд!");
	SetTimer("Ddchat", 10000, 0);
	dchat = 0;
	if(!GetPVarInt(playerid,"Fraction_Duty")) return true;
	switch(PlayerInfo[playerid][pMember])
	{
		case 1,2,3,4,7,10,19,21,11:
		{
			if(GetString(f_rank[PlayerInfo[playerid][pMember] - 1][PlayerInfo[playerid][pRank] - 1], "None")) format(cmdstr, sizeof(cmdstr), "[%s] %s %s[%d]: (( %s ))",GetNameMember(playerid),GetRank(playerid), Name(playerid),playerid, params[0]);
			else format(cmdstr, sizeof(cmdstr), "[%s] %s %s[%d]: (( %s ))",GetNameMember(playerid),f_rank[PlayerInfo[playerid][pMember] - 1][PlayerInfo[playerid][pRank] - 1],  Name(playerid),playerid, params[0]);
			SendTeamMessage(1, COLOR_ALLDEPT, cmdstr);
		}
		default: return SCM(playerid,COLOR_GREY,"Вам недоступна данная функция");
	}
	return true;
}
