publics: SpeedoUpdate(playerid)
{
	if(IsPlayerConnected(playerid) && IsPlayerInAnyVehicle(playerid))
	{
		if(GetPlayerState(playerid) == 2)
		{
  			new tacha = GetPlayerVehicleID(playerid), vehicleid = tacha;
			new stext[35];
			new Float:speedcar = GetVehicleSpeed_(tacha);
			new vname[32];
			if(veh_info[tacha - 1][v_stability_boost] == 1) format(vname, sizeof(vname), "%s", VehicleNames[GetVehicleModel(tacha)-400]);
			else format(vname, sizeof(vname), "%s ~g~[TURBO GT]", VehicleNames[GetVehicleModel(tacha)-400]);
			PlayerTextDrawSetString(playerid, TDEditor_PTD[playerid][6], vname);
			
			format(stext,sizeof(stext),"%.0f",speedcar);
			PlayerTextDrawSetString(playerid, TDEditor_PTD[playerid][0], stext);
			if(!ActualElectroCar(tacha)) format(stext, sizeof(stext), "%.0f", veh_info [ tacha - 1 ] [ v_fuel ]);
			else
			{
				format(stext, sizeof(stext), "~g~ELECTRO");
				TextDrawHideForPlayer(playerid, TDEditor_TD[17]);
			}	
			PlayerTextDrawSetString(playerid, TDEditor_PTD[playerid][3], stext);
            switch(SpeedVehicle(playerid))
 			{
       			case 0..1: format(stext,sizeof(stext),"~w~~h~IIII");
       			case 2..30: format(stext,sizeof(stext),"~w~~h~IIIIII");
	            case 31..50: format(stext,sizeof(stext),"~w~~h~IIIIIIII");
	            case 51..67: format(stext,sizeof(stext),"~y~~h~IIIIIIIIIII");
	            case 68..80: format(stext,sizeof(stext),"~y~~h~IIIIIIIIIIIIII");
	            case 81..98: format(stext,sizeof(stext),"~r~~h~IIIIIIIIIIIIIIIIII");
	            case 99..110: format(stext,sizeof(stext),"~r~~h~IIIIIIIIIIIIIIIIIIIII");
	            case 111..300: format(stext,sizeof(stext),"~r~~h~IIIIIIIIIIIIIIIIIIIIIIII");
	            default: format(stext,sizeof(stext),"~r~~h~IIIIIIIIIIIIIIIIIIIIIIII");
     		}
           	PlayerTextDrawSetString(playerid,TDEditor_PTD[playerid][2],stext);
           	new vehe[20];
           	if(veh_info [ tacha - 1 ] [ v_locked ]) vehe = "~r~LOCK";
			else vehe = "~g~OPEN";
			format(stext,sizeof(stext),"%s",vehe);
			PlayerTextDrawSetString(playerid,TDEditor_PTD[playerid][4],stext);
			if(PlayerMobile[playerid])
			{
				new speedMobile[128];
				format(speedMobile,sizeof(speedMobile)," ~n~ ~n~ ~n~ ~n~ ~n~ ~n~ ~r~ %.0f KM/H ~n~ ~p~ FUEL: %.0f ~n~ %s", speedcar, veh_info [ tacha - 1 ] [ v_fuel ], vehe);
				GameTextForPlayer(playerid, speedMobile, 1000, 3);
			}
			if(SpeedVehicle(playerid) >= 350 && PlayerInfo[playerid][pAdmin] == 0) SCM(playerid, COLOR_REDD, "{FF0000}Вы были кикнуты за использования постороннего ПО {FFFFFF}(Speed Hack #2)"), Kick(playerid);
			if(veh_info [ tacha - 1 ] [ v_fuel ] <= 0 && !v_velo ( tacha ) && tacha != 485)
			{
  				veh_info [ tacha - 1 ] [ v_fuel ] = 0;
  				new engine, lights, alarm, doors, bonnet, boot, objective;
  				GetVehicleParamsEx(tacha,engine,lights,alarm,doors,bonnet,boot,objective);
  				SetVehicleParamsEx(tacha,VEHICLE_PARAMS_OFF,lights,alarm,doors,bonnet,boot,objective);
  				zavodis[GetPlayerVehicleID(playerid)] = 0;
  				GameTextForPlayer(playerid,"~r~FUEL HAS ENDED", 5000, 3);
   			}
			new Float:_coord[3];
			GetVehicleVelocity(vehicleid,_coord[0],_coord[1],_coord[2]);
			new Float:v_speed = floatsqroot(floatpower(floatabs(_coord[0]), 2.0)+ floatpower(floatabs(_coord[1]), 2.0)+ floatpower(floatabs(_coord[2]), 2.0))* 100.3 ;
			new Float:carhp;
			GetVehicleHealth(tacha, carhp);
			new Float:speed = SpeedVehicle(playerid);
			if(veh_info[vehicleid - 1][v_engine_boost] > 0 && !veh_info[vehicleid - 1][v_stability_boost] && speed > 30 && player_holding_key_down[playerid][0] && speed < max_veh_speed[GetVehicleModel(vehicleid)- 400] - 50)
			{
				new Float:coef = (speed - veh_info[vehicleid - 1][v_old_speed]) * 200/speed;
				if(coef > 0)
				{ 
					if(speed >= MaxSpeedCar[GetVehicleModel(vehicleid)- 400]) coef = floatdiv(coef, veh_info[vehicleid - 1][v_engine_boost]*2.5);
					else coef = floatdiv(coef, veh_info[vehicleid - 1][v_engine_boost]);
					SetVehicleSpeed(vehicleid, speed + coef);
				}	
			}
			else if(player_holding_key_down[playerid][1] && speed > 100)
			{
				if(veh_info[vehicleid - 1][v_engine_boost] > 0 && !veh_info[vehicleid - 1][v_stability_boost] && veh_info[vehicleid - 1][v_old_speed] > 40 &&(_coord[2] - veh_info[vehicleid - 1][v_old_zangle] < 0.3 && _coord[2] - veh_info[vehicleid - 1][v_old_zangle] > - 0.3)&& 1000 >= GetTickCount()- player_holding_key_down[playerid][0] >= 150)
				{
					SetVehicleSpeed(vehicleid, v_speed - veh_info[vehicleid - 1][v_brake_boost]);
					veh_info[vehicleid - 1][v_old_zangle] = _coord[2];
				}
			}
			veh_info[vehicleid - 1][v_old_zangle] = _coord[2];
			veh_info[vehicleid - 1][v_old_speed] = v_speed;
			if(carhp < 290)
			{
				if(VG[vehicleid][vgDrugs])
				{
					SendGhettoMessage(COLOR_RED, "ВНИМАНИЕ! Партия наркотиков уничтожена");  
					SendCopsMessage(COLOR_GREEN2, "ВНИМАНИЕ! Партия наркотиков уничтожена, хорошая работа! Премия +$500");
					VG[vehicleid][vgDrugs] = 0;
					TimeToDrug = 1;
				}
				new engine, lights, alarm, doors, bonnet, boot, objective;
 				GameTextForPlayer(playerid,"~r~ENGINE HAS BROKENED", 5000, 3),enginebrokened[tacha] = 1, okSetVehicleHealth(tacha, 300.0);
				GetVehicleParamsEx(tacha,engine,lights,alarm,doors,bonnet,boot,objective);
				SetVehicleParamsEx(tacha,false,false,alarm,doors,bonnet,boot,objective);
			}
			if(carhp > 450 && carhp < 600 && !mp_fall[playerid])
			{
				if(SpeedVehicle(playerid) > 40 && tacha != 485) SetVehicleSpeed(tacha, 40);
				if(!GetPVarInt(playerid, #SPEEDREDUCED)) GameTextForPlayer(playerid,"~b~Vehicle Damaged. Speed Reduced!", 3000, 3);
				SetPVarInt(playerid, #SPEEDREDUCED, 1);
			}
			if(carhp > 300 && carhp < 450 && !mp_fall[playerid])
			{
				if(SpeedVehicle(playerid) > 15 && tacha != 485) SetVehicleSpeed(tacha, 15); 
				if(GetPVarInt(playerid, #SPEEDREDUCED) != 2) GameTextForPlayer(playerid,"~r~Vehicle Damaged. Speed Reduced!!!", 3000, 3);
				SetPVarInt(playerid, #SPEEDREDUCED, 2);
			}
			if(derby_status && carhp < 350) {
				if(mp_fall[playerid]) {
					if(PlayerInfo[playerid][pTempVeh]!=-1) DestroyVehicle(PlayerInfo[playerid][pTempVeh]);
					SendClientMessage(playerid, 0xFF4F00FF, "Вы проиграли в мероприятии");
					mp_fall[playerid] = false;
					SpawnPlayer(playerid);
					SetPlayerInterior(playerid, 0);
					derby_count --;
					if(derby_count == 1) {
						derby_count = 0;
						derby_status = false;
						SendClientMessageToAll(-1, "[МП] {"#cBLUES"}Мероприятие дэрби {319AFF}завершилось.");
						mp_active = false;
						mp_fall_start = 0;
						mp_active = false;
						new player_name[MAX_PLAYER_NAME];
						foreach(new i : Player) {
							if(!mp_fall[i])
								continue;
							if(mp_fall[i]) {
								GetPlayerName(i, player_name, MAX_PLAYER_NAME);
								format(cmdstr, 72, "[МП] {"#cBLUES"}Победитель: %s", player_name);
								SendClientMessageToAll(-1, cmdstr);
								mp_fall[i] = false;
								SendClientMessage(i, 0x099619FF, "Поздравляем! Вы остались последним на дэрби и получаете свою награду!");
								SendClientMessage(i, 0xffd100FF, "Награда: {ffffff}30.000$");
								if(PlayerInfo[i][pTempVeh]!=-1) DestroyVehicle(PlayerInfo[i][pTempVeh]);
								SpawnPlayer(i);
								SetPlayerInterior(i, 0);
								GiveMoney(i, 30000, "Новое МП 2");
							}
						}	
					}
				}	
			}
			SetTimerEx("SpeedoUpdate",1000,0,"i",playerid);	
		}
	}
	return true;
}
