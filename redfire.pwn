stock LeaveJobZD(playerid)
{
	if(GetPVarInt(playerid, #InJobZD))
	{
		if(GetPVarInt(playerid, #remzp) != 0)
		{
		    new string_[144];
			format(string_, sizeof string_, "Tkven Daainstaliret Nawilebi "P"%d"W" Da Gamoimushava {33aa33}$%d",GetPVarInt(playerid, #remzp), GetPVarInt(playerid, #remzp)*300);
			SendClientMessage(playerid, COLOR_WHITE, string_);
			SendClientMessage(playerid, COLOR_GREEN, !"Tkven Warmatebit Daasrulet Samushao Shegidzliat Fuli Aigot");
			GiveMoney(playerid, GetPVarInt(playerid, #remzp)*300, "Rkinigzis Remontis Xelpasi");
		}
		else SendClientMessage(playerid, COLOR_GREEN, !"Samushao Dge Dasrulda!");
		DeletePVar(playerid, #InJobZD);
		DeletePVar(playerid, #remzp);
		SetPlayerSkinEx(playerid,GetPVarInt(playerid,#JobSkin));
		DisablePlayerCheckpoint(playerid);
		DeletePVar(playerid, #JobSkin);
		if(IsPlayerAttachedObjectSlotUsed(playerid, 0)) RemovePlayerAttachedObject(playerid, 0);
		if(IsPlayerAttachedObjectSlotUsed(playerid, 1)) RemovePlayerAttachedObject(playerid, 1);
	}
	return 1;
}
