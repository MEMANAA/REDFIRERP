publics: unitpay_callback(playerid)
{
	new rows,sum, status, did;
    cache_get_row_count(rows);
	if(!rows) return true;
	cache_get_value_name_int(0, "id", did);
	cache_get_value_name_int(0, "sum", sum);
	cache_get_value_name_int(0, "status", status);
    if(status == 1)
    {
        PlayerInfo[playerid][pDonatemoney] += sum; // x2
        if(sum >= 300)
		{
		    PlayerInfo[playerid][pDonatemoney] += sum/10;
		    SCMF(playerid, COLOR_GREEN, "Акция: Вам начислено %d COIN", sum/10);
			printf("Акция: %s начислено: %d", Name(playerid), sum/10);
  		}
		else if(sum >= 500)
		{
		    PlayerInfo[playerid][pDonatemoney] += sum/5;
		    SCMF(playerid, COLOR_GREEN, "Акция: Вам начислено %d COIN", sum/5);
		    printf("Акция: %s начислено: %d", Name(playerid), sum/5);
		}
		else if(sum >= 1000)
		{
		    PlayerInfo[playerid][pDonatemoney] += sum/3;
		    SCMF(playerid, COLOR_GREEN, "Акция: Вам начислено %d COIN", sum/3);
		    printf("Акция: %s начислено: %d", Name(playerid), sum/3);
		}
		UpdatePlayerData(playerid, "pDonatemoney", PlayerInfo[playerid][pDonatemoney]);
		string[0] = EOS;
		format(string, 256, "{FFFFFF}%s\nНа Ваш счёт зачислено %i Coin.\n{FFFF00}Спасибо за помощь серверу!", Name(playerid), sum); // x2
		ShowPlayerDialogEx(playerid, DIALOG_UNUSED, 0, "{FFFFFF}Пополнение счета", string, "Daxurva","");
		format(cmdstr, sizeof(cmdstr), "DELETE FROM `freekassa_payments` WHERE `id` = '%d'", did);
		mysql_tquery(connects, cmdstr);
    }
	return 1;
}
