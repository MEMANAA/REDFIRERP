CMD:unloading(playerid, params[])
{
    if(!IsAGang(playerid)) return 1;
    if(GetPVarInt(playerid, "AFK_Time") > 1) return true;
    new vehicleid = GetPlayerVehicleID(playerid), string_[128];
	new fraction_ = PlayerInfo[playerid][pMember];

	if(GetPlayerState(playerid) == PLAYER_STATE_PASSENGER) return err(!"[Shecdoma]: {AFAFAF}Нельзя использовать на пассажирском месте.");
	//if(GetVehicleModel(vehicleid) != 482) return err(!"[Shecdoma]: {AFAFAF}Вы не в фургоне!");
	if(!gVehicleGun[vehicleid][vAllow]) return err(!"[Shecdoma]: {AFAFAF}Вы должны быть фургоне своей банды!");
	if(PlayerInfo[playerid][pRank] < 3)return SendClientMessage(playerid,COLOR_GRAD1,"Доступно только с 3 ранга!");
	if(gVehicleGun[vehicleid][vGunAmmo] <= 0) return err(!"[Shecdoma]: {AFAFAF}Не достаточно материалов, для разгрузки");
	if(PlayerToPoint(6.0,playerid,1454.3406,758.1638,11.0234))
	{
		FI[5][fMaterials] += gVehicleGun[vehicleid][vGunAmmo];
		gVehicleGun[vehicleid][vGunAmmo] = 0;
		format(string_, sizeof(string_), "Материалы разргружены! В фургоне: %d/10000 матов", gVehicleGun[vehicleid][vGunAmmo]);
		SendClientMessage(playerid, TEAM_GROVE_COLOR, string_);
		if(FI[5][fMaterials] > 200000) FI[5][fMaterials] = 200000;
		format(string_, sizeof(string_), "Склад мафии LCN: %d/200000", FI[5][fMaterials]);
		return SCM(playerid, COLOR_WHITE, string_);
	}
	else if(PlayerToPoint(6.0,playerid,967.3965,1684.1112,8.8516))
	{
		FI[14][fMaterials] += gVehicleGun[vehicleid][vGunAmmo];
		gVehicleGun[vehicleid][vGunAmmo] = 0;
		format(string_, sizeof(string_), "Материалы разргружены! В фургоне: %d/10000 матов", gVehicleGun[vehicleid][vGunAmmo]);
		SendClientMessage(playerid, TEAM_GROVE_COLOR, string_);
		if(FI[14][fMaterials] > 200000) FI[14][fMaterials] = 200000;
		format(string_, sizeof(string_), "Склад Русской мафии: %d/200000", FI[14][fMaterials]);
		return SCM(playerid, COLOR_WHITE, string_);
	}
	else if(PlayerToPoint(6.0,playerid,1460.3116,2763.8110,10.8203))
	{
		FI[6][fMaterials] += gVehicleGun[vehicleid][vGunAmmo];
		gVehicleGun[vehicleid][vGunAmmo] = 0;
		format(string_, sizeof(string_), "Материалы разргружены! В фургоне: %d/10000 матов", gVehicleGun[vehicleid][vGunAmmo]);
		SendClientMessage(playerid, TEAM_GROVE_COLOR, string_);
		if(FI[6][fMaterials] > 200000) FI[6][fMaterials] = 200000;
		format(string_, sizeof(string_), "Склад Якудза: %d/200000", FI[6][fMaterials]);
		return SCM(playerid, COLOR_WHITE, string_);
	}
	if(!IsATerra (playerid)) return SendClientMessage(playerid, COLOR_GRAD1, "Вы не на своей базе");
	if(fraction_ == 12)
	{
	    if(FI[12][fMaterials] > 200000) return err(!"[Shecdoma]: {AFAFAF}Склад заполнен");
		FI[12][fMaterials] += gVehicleGun[vehicleid][vGunAmmo];
	}
	if(fraction_ == 13)
	{
	    if(FI[13][fMaterials] > 200000) return err(!"[Shecdoma]: {AFAFAF}Склад заполнен");
		FI[13][fMaterials] += gVehicleGun[vehicleid][vGunAmmo];
	}
    if(fraction_ == 15)
	{
	    if(FI[15][fMaterials] > 200000) return err(!"[Shecdoma]: {AFAFAF}Склад заполнен");
		FI[15][fMaterials] += gVehicleGun[vehicleid][vGunAmmo];
	}
    if(fraction_ == 17)
	{
	    if(FI[17][fMaterials] > 200000) return err(!"[Shecdoma]: {AFAFAF}Склад заполнен");
		FI[17][fMaterials] += gVehicleGun[vehicleid][vGunAmmo];
	}
    if(fraction_ == 18)
	{
	    if(FI[18][fMaterials] > 200000) return err(!"[Shecdoma]: {AFAFAF}Склад заполнен");
		FI[18][fMaterials] += gVehicleGun[vehicleid][vGunAmmo];
	}
	format(string_, sizeof(string_), "[F] %s[%d] разгрузил %d материалов на склад банды",Name(playerid), playerid, gVehicleGun[vehicleid][vGunAmmo]);
	SendFamilyMessage(fraction_,COLOR_GREEN, string_);
	CrimeStat[playerid][1] += gVehicleGun[vehicleid][vGunAmmo];
	update_crime_data ( playerid );
	PlayerInfo[playerid][CrimeProgress] += 1;
	CheckCrimeProgress(playerid);

	//FI[PlayerInfo[playerid][pMember]][fRepute] ++;

	if ( quest_status [ playerid ] [ 10 ] == 1 )
	{
		quest_progress [ playerid ] [ 10 ] += gVehicleGun[vehicleid][vGunAmmo] ;
		if ( quest_progress [ playerid ] [ 10 ] >= 20000 )
		{
			quest_status [ playerid ] [ 10 ] = 2 ;
			ShowPlayerDialogEx(playerid,0,DIALOG_STYLE_MSGBOX,"{"#cBLUES"}Informacia", !"{FFFFFF}\n\tЗадание успешно Dasrulebuliо. Введите /fquest, чтобы получить своё вознаграждение.\n","Daxurva","");
			SendClientMessage ( playerid, col_succes, "Задание успешно Dasrulebuliо. Введите /fquest, чтобы получить своё вознаграждение." ) ;
		}
		update_quest_data ( playerid ) ;
	}

	gVehicleGun[vehicleid][vGunAmmo] = 0;


	return SCM(playerid,COLOR_GREEN, "Материалы разгружены");
}
