CMD:id(playerid, params[])
{
	new tmp[64];
	if(sscanf(params, "s[64]", tmp)) return SCM(playerid, -1, "Введите: /id [имя игрока]");
	SCM(playerid,0x6495EDFF,"Игроки онлайн:");
	new id=-1,name[84];
	if(!isNumeric(tmp))
	{
		foreach(new i: Player)
		{
			if(!IsPlayerConnected(i)) continue;
			GetPlayerName(i,name,30);
			if(strfind(name,tmp,true) != -1)
			{
				if(id >= 4) {
					id++;
					continue;
				}
				if(GetPVarInt(i,"AFK_Time") > 3) format(name,sizeof(name),"%s [%d] [LVL: %d] [AFK: %s]",name,i, PlayerInfo[i][pLevel], Convert(GetPVarInt(i,"AFK_Time")));
				else format(name,sizeof(name),"%s [%d] [LVL: %d]",name,i, PlayerInfo[i][pLevel]);
				SCM(playerid,COLOR_WHITE,name);
				id++;
			}
		}
		if(id == -1) return SCM(playerid,COLOR_WHITE,"Не найдено");
		else if(id >= 4) {
			format(name,sizeof(name),"Показано 5 найденных из %d", id-2);
			SCM(playerid, COLOR_GRAD1, name);
		}
	}
	else
	{
		id = strval(tmp);
		if(!IsPlayerConnected(id)) return SCM(playerid,COLOR_WHITE,"Неверный ID");
		GetPlayerName(id,name,24);
		if(GetPVarInt(id,"AFK_Time") > 3) format(name,sizeof(name),"%s [%d] [LVL: %d] [AFK: %s]",name,id, PlayerInfo[id][pLevel], Convert(GetPVarInt(id,"AFK_Time")));
		else format(name,sizeof(name),"%s [%d] [LVL: %d]",name,id, PlayerInfo[id][pLevel]);
		SCM(playerid,COLOR_WHITE,name);
	}
	return 1;
}
