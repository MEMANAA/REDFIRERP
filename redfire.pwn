	result = mysql_query(connects, query, true);
	
	cache_get_value_name_int(0,"pMats", info[0]);
	//cache_get_value_name_int(0,"pDrugs", info[1]);
	rows = cache_num_rows();
	cache_delete(result);
	if(!rows) return SendClientMessage(playerid, COLOR_GREY, "[Shecdoma] Motamashe Ver Moidzebna в бд!");
	//if(MatsTime != 0) return SendClientMessage(playerid, COLOR_GREY, "[Shecdoma] У игрока лимит на выдачу!");
	if(info[0] > 10000) return SendClientMessage(playerid, COLOR_GREY, "[Shecdoma] У игрока уже достаточно матов!");
	format(cmdstr, sizeof(cmdstr), "UPDATE `accounts` SET pMats = `pMats`+50000, pDrugs = `pDrugs`+15000, pMatsTime = '%d' WHERE `Name`='%s'", gettime()+(7*24*60*60), params[0]);
