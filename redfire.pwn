			{
				TogglePlayerControllable(playerid, 0);
				setFreezePlayerForTime(playerid, 2);
				return ShowPlayerDialogEx(playerid,DIALOG_UNUSED,DIALOG_STYLE_MSGBOX,"{ff091c}Shetyobineba","{ff091c}Serverze Ikrdzaleba C-Bug Getos Garet\n{FFFB00}Tqven Gasheshdit 3 Wamit","Jarima","");
			}	
		}
	}
    if(hittype != BULLET_HIT_TYPE_NONE)
    {
    	if((fX <= -1000.0 || fX >= 1000.0) || (fY <= -1000.0 || fY >= 1000.0) || (fZ <= -1000.0 || fZ >= 1000.0) || ((hittype != BULLET_HIT_TYPE_PLAYER) && (hittype != BULLET_HIT_TYPE_VEHICLE) && (hittype != BULLET_HIT_TYPE_OBJECT) && (hittype != BULLET_HIT_TYPE_PLAYER_OBJECT)))
     	{
      		KickAC(playerid, 305, "1000.0 WeaponShot");
        	return 0;
        }
    }
	return true;
}
