			} 
		}
		PlayerInfo[i][pExp]++;
		if(PlayerInfo[i][pDonateRank] > 4) PlayerInfo[i][pExp]++;
		if(Conf[2] == 1 && PlayerInfo[i][pLevel] < 6) PlayerInfo[i][pExp]++; //x2 xD
