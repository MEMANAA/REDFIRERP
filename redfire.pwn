CMD:tazer(playerid) {
	if(!IsACop(playerid)) return SCM(playerid, -1, "Xelmisaccvdomia Mxolod SamartalDamcavi Organoebistvis");
	string[0] = EOS;
	if(GetPVarInt(playerid,"tazershoottime") > gettime()) {
		format(string,128,"Daelodet Tazeris Gadatvirtvas [%d Wami]",GetPVarInt(playerid ,"tazershoottime") - gettime());
		SCM(playerid,-1,string);
		return 1;
	}
	if(PlayerInfo[playerid][tTazers][0] > 0) return SCM(playerid, -1, "Iaragi Ukve Datenilia");
	for new i=0;i<sizeof string;i++ do string[i]=EOS;
	for(new i; i <= 11; i++) {
		new gunid, ammo, storeslot = -1;
		GetPlayerWeaponData(playerid,i,gunid,ammo);
		switch(gunid) {
			case 24 : storeslot = 1;
			case 25 : storeslot = 1;
		}
		if(!gunid || !ammo || storeslot == -1) continue;
		new gunname[32];
		GetWeaponName(gunid,gunname,32);
		format(string,128,"%s\n%s [%d]",string,gunname,ammo);
	}
	if(!strlen(string)) return SCM(playerid, -1, "Iaragi Ar Gakvs");
	ShowPlayerDialogEx(playerid,D_TAZER,DIALOG_STYLE_LIST,""P"Iaragi",string,"Datenva","Gaukmeba");
	return 1;
}
