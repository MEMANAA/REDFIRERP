	{
    	if(areaid == DrugsData[i][objectzone])  DeletePVar(playerid, #OBJ_ID);
		break;
    }
	if(areaid == zoneAvia[0] || areaid == zoneAvia[1] || areaid == zoneAvia[2] || areaid == zoneAvia[3] || areaid == zoneAvia[4] || areaid == zoneAvia[5] || areaid == zoneAvia[6]){
		if(PlayerInfo[playerid][pJob] != 9) return 1;
		if ( GetVehicleDistanceFromPoint (BotVehicles[0], 1940.1781,-2301.5740,13.4670 ) < 1.0 )
		{
			SendClientMessage(playerid,COLOR_RED,"[Minishneba] "WHITE"Вы покинули место работы, вернитесь чтобы продолжить");
		}	
	}	 
    if(areaid == gAreas[arDuel][1]){
		if(DuelInfo[GetPVarInt(playerid, #di_ID)][dActivate] && DuelInfo[GetPVarInt(playerid, #di_ID)][dActivate] != 3 && DuelInfo[GetPVarInt(playerid, #di_ID)][diID] == playerid){
	        LeaveIsDuel(playerid);
			return	SendClientMessage(playerid, COLOR_YELLOW, "SMS: Эй! Тебя не видно на районе, твое участие в дуэли прервано. Отправитель: Организатор");
		}
    }
    if(areaid >= TagInfo[1][tSphere] && areaid <= TagInfo[GRAFFITI_COUNT-1][tSphere])
	{
	    if(PlayerInfo[playerid][PlayerSpray] == true)
	    {
			PlayerInfo[playerid][PlayerSpray] = false;
		    DeletePVar(playerid,"ColorFire");
		}
		DeletePVar(playerid,"TagInfoID");
	}
	if(areaid >= area_golod[0] && areaid <= area_golod[20]) {
		if(time_gamegolod && player_to_golod[playerid]) {
			new Float:POS[3];
		    GetPlayerPos(playerid, POS[0], POS[1], POS[2]);
		    CreateExplosionForPlayer(playerid, POS[0], POS[1], POS[2], 7, 5.0);
		}
	}
	return 1;
}
