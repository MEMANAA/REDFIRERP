	hours = number / 3600;
	mins = (number - hours * 3600) / 60;
	format(stringt, 30, "%d ч %d мин", hours, mins);
	return stringt;
}
stock ConvertMinutes(number)
{
	new hours = 0, mins = 0, stringt[30];
