publics: OnCheatDetected(playerid, const ip_address[], type, code, code2) {
	new npcname[MAX_PLAYER_NAME+1];
	GetPlayerName(playerid, npcname, sizeof(npcname));
	if(!strcmp(npcname, "Pilot", true)){
			PlayerInfo[playerid][pNPC] = 1;
	}
	else if(!strcmp(npcname, "Bus_Driver", true)){
		PlayerInfo[playerid][pNPC] = 1;
	}
	else if(!strcmp(npcname, "Bus_Lucifer", true)){
		PlayerInfo[playerid][pNPC] = 1;
	}
	if(PlayerInfo[playerid][pNPC]) return EnableAntiCheatForPlayer(playerid, code, 0);
    if(PlayerInfo[playerid][pAdmin] > 0) return EnableAntiCheatForPlayer(playerid, code, 0);//true;
	if(PlayerMobile[playerid] && code == 50) return EnableAntiCheatForPlayer(playerid, code, 0);//true;
	if(PlayerMobile[playerid] && code == 51) return EnableAntiCheatForPlayer(playerid, code, 0);//true;
	if(PlayerMobile[playerid] && code == 4) return EnableAntiCheatForPlayer(playerid, code, 0);//true;
	if(PlayerMobile[playerid] && code == 47) return EnableAntiCheatForPlayer(playerid, code, 0);
    new ACName[54][] = {
	"AirBreak (onfoot) [#000]","AirBreak (in vehicle) [#001]","AirBreak/Teleport [#002]","Teleport hack (in vehicle) [#003]","Teleport hack (into/between vehicles) [#004]",
	"Teleport hack (vehicle to player) [#005]","Teleport hack (pickups) [#006]","FlyHack [#007]","FlyHack (in vehicle) [#008]","SpeedHack (onfoot) [#009]",
	"SpeedHack (in vehicle) [#010]","Repair Car [#011]","Health hack [#012]","Armour hack [#013]","Money hack [#014]","Weapon hack [#015]",
	"Ammo Hack [#016]","Ammo Hack [#017]","Special actions hack [#018]","GodMode [#019]","GodMode [#020]",
	"Invisible hack [#021]","lagcomp-spoof [#022]","Tuning hack [#023]","Parkour mod [#024]","Quick turn [#025]","Rapid fire [#026]",
	"FakeSpawn [#027]","FakeKill [#028]","Pro Aim [#029]","CJ run [#030]","CarShot [#031]","CarJack [#032]","UnFreeze [#033]",
	"AFK Ghost [#034]","Full Aiming [#035]","Fake NPC [#036]","Reconnect [#037]","High ping [#038]","Dialog hack [#039]","Protection from the sandbox [#040]",
	"Protection against an invalid version [#041]","Rcon hack [#042]","Tuning crasher [#043]","Invalid seat crasher [#044]","Dialog crasher [#045]",
	"Attached object crasher [#046]","Weapon Crasher [#047]","Flood protection connects to one slot [#048]","flood callback functions [#049]",
	"flood change seat [#050]","Ddos [#051]","NOP's [#052]","Fast movement [#053]"  };
	
	if(playerid != INVALID_PLAYER_ID)
	{
		switch(code)
		{
			case 5, 6, 14, 22, 32, 39: return 1;
			case 38: {
				err(!"ВНИМАНИЕ!"W" У Вас слабое интернет соединение");
				err(!"Для более комфортной игры необходимо оптимизировать работу сетевого экрана ПК");
				err(!"А также, настоятельно рекомендуем Вам проверить ПК на присутствие вредоносного ПО");
				return true;
			}
			case 11: if(!warning_kick_ac) return SendWarningCheatMessageToAdmin(playerid,"Восстановление здоровья - Veh");
			case 18: if(!warning_kick_ac) return SendWarningCheatMessageToAdmin(playerid,"Special actions hack");	
			case 27: if(!warning_kick_ac) return SendWarningCheatMessageToAdmin(playerid,"FakeSpawn");		
			case 28: if(!warning_kick_ac) return SendWarningCheatMessageToAdmin(playerid,"FakeKill");
			case 15:
				{
					SendWarningCheatMessageToAdmin(playerid,"Weapon Hack");
					return DelGun(playerid);
				}
			case 40: {
				err(MAX_CONNECTS_MSG);
				AntiCheatKickWithDesync(playerid, 40);
				return true;
			}
			case 41: {
				err(UNKNOWN_CLIENT_MSG);
				//BlockIpAddress(ip_address, 120 * 1000); // block the connections from this ip for one minute
				AntiCheatKickWithDesync(playerid, 41);
				new plrIP[16];
    			GetPlayerIp(playerid, plrIP, sizeof(plrIP));
				BlockIpAddress(plrIP, 5000 * 1000);
				return true;
			}
			case 43..46: {
				DelGun(playerid);
				Kick(playerid);
				return true;
			}
			/*case 47:
			{
				//return SendWarningCheatMessageToAdmin(playerid,"Weapon Crasher");
                printf("[Античит] %s использует Weapon Crasher (code 1: #%d | code 2: #%d)", Name(playerid), code, code2);
			    if(!IsPlayerDying(playerid) && !GetPVarInt(playerid,"PlayeridDeath") && GetPVarInt(playerid, "AFK_Time") < 2)
			    {
			        DelGun(playerid);
					SCM(playerid, -1, "Ваше оружие удалено по подозрению в читерстве");
			        if(code != 2 && code2 != 2) AntiCheatKickWithDesync(playerid, 47);
			    }
				return 1;
			}*/
			case 48: if(!warning_kick_ac) return SendWarningCheatMessageToAdmin(playerid,"Flood protection connects to one slot");
			case 30: {
				PlayerSpawn(playerid);
				return true;
			}
		}
		new acHour, acMin, acSec;
		gettime(acHour, acMin, acSec);

		format(cmdstr, sizeof(cmdstr), "[Античит] %s был кикнут %s (code 1: #%d | code 2: #%d)", Name(playerid), ACName[code], code, code2);
		SendAdminMessage(COLOR_GREY, cmdstr);
		WriteLog("KickCheat.txt", cmdstr);
		SCMF(playerid,COLOR_REDD," {FF0000}Вы были кикнуты за использования постороннего ПО {FFFFFF} %s", ACName[code]);
		SendClientMessage(playerid,-1," Если произошла Shecdoma, отпишитесь на форуме (тех. раздел): "NameForum"");
		SCMF(playerid, COLOR_REDD, " {CECECE}Время на момент срабатывания: {9ACD32}%02d:%02d:%02d {CECECE}| Потери: %.2f", acHour, acMin, acSec, NetStats_PacketLossPercent(playerid));

	    //SCMF(playerid, COLOR_REDD, " IP: %d | Type: %d | Code: %d | Code2: %d", ip_address, type, code, code2);
	    AntiCheatKickWithDesync(playerid, code);
	}
	return true;
}
