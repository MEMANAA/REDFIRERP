CMD:contract(playerid, params[])
{
	if(sscanf(params, "ud", params[0], params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите /contract [id игрока] [сумма]");
	if(PlayerInfo[params[0]][pAdmin] > 0) return SendClientMessage(playerid, 0xAFAFAFAA, "Вы не можете заказать данного человека.");
	if((params[1] < 10000) || (params[1] > 10000000)) return SendClientMessage(playerid, 0xAFAFAFAA, "Сумма контракта должна быть минимум $10000, и максимум $1000000!");
	if(PlayerInfo[playerid][pMember] == 24) return SendClientMessage(playerid, 0xAFAFAFAA, "Вы не можете заказать человека.");
	if(PlayerInfo[params[0]][pMember] == 24) return SendClientMessage(playerid, 0xAFAFAFAA, "Вы не можете заказать данного человека.");
	if(IsACop(params[0]) && (params[1] < 50000)) return SendClientMessage(playerid, 0xAFAFAFAA, "Голова данного человека стоит больше чем вы предлогаете.");
	if(params[0] == playerid) return SendClientMessage(playerid, 0xAFAFAFAA, "Вы не можете заказать себя!");
	if(PlayerInfo[params[0]][pContract] > 0) return SendClientMessage(playerid, 0xAFAFAFAA, "Данная жертва уже была заказана, хитманы не могут её Datanxmeba.");
	if(PlayerInfo[playerid][pCash] < params[1]) return SendClientMessage(playerid, 0xAFAFAFAA, "Вы решили нас обмануть? У вас нету денег!");
	PlayerInfo[playerid][pCash] -= params[1];
	PlayerInfo[params[0]][pHeadValue] += params[1];
	new stringer[128];
	format(stringer, sizeof(stringer), "Внимание! Поступил новый заказ, цена головы - *%d*", params[1]);
	SendFamilyMessage(24, COLOR_YELLOW, stringer);
	SCMF(playerid, COLOR_WHITE, "Вы успешно заказали %s за %d $.", PlayerInfo[params[0]][pName], params[1]);
	PlayerInfo[params[0]][pContract] = 600;
	return true;
}
