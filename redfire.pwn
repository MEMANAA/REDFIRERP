			PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
			ProxDetector(playerid, "достаёт мобильный телефон");
			format(cmdstr, sizeof(cmdstr), "SMS: %s. Получатель: %s[%d]",params[1],Name(params[0]),params[0]);
			//SCM(playerid,  COLOR_YELLOW, cmdstr);
