stock EnableGPSForPlayerCar(playerid, Float:x, Float:y, Float:z, Float:a)
{
	SetPlayerCheckpoint(playerid, Float:x, Float:y, Float:z, 5.0);

	if(rfaggio[playerid] != INVALID_VEHICLE_ID)
	{
		DestroyVehicle(rfaggio[playerid]);
		rfaggio[playerid] = INVALID_VEHICLE_ID;
	}
	new rand = Random(0, 4), trailerid;
	switch (rand)
	{
		case 0: trailerid = _CreateVehicle(606, x, y, z, a, -1, -1, 60);
		case 1: trailerid = _CreateVehicle(607, x, y, z, a, -1, -1, 60);
		case 2: trailerid = _CreateVehicle(607, x, y, z, a, -1, -1, 60);
