			captures--;
			capture_now[capture_band( GZInfo[i][gNapad] )] = -1;
			capture_now[capture_band( GZInfo[i][gFrakVlad] )] = -1;
			CountOnZone[GZInfo[i][gNapad]] = 0;
			CountOnZone[GZInfo[i][gFrakVlad]] = 0;
			ClearTotalGz();
			ZoneOnBattle[i] = 0;
			GZInfo[i][gNapad] = 0;
			GangZoneStopFlashForAll(GZInfo[i][gID]);
			GangZoneHideForAll(GZInfo[i][gID]);
			GangZoneShowForAll(GZInfo[i][gID],GetGangZoneColor(i));
			KillTimer(GZInfo[i][gTimer]);
			GZInfo[i][gTimer] = 0;
