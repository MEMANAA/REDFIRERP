	printf("[DEBUG][OnPlayerEnterVehicle]playerid:%d nick: %s vehicleid: %d ispassenger: %d", playerid, Name(playerid), vehicleid, ispassenger);
    if(connect_mserver == 1) SCMF(playerid,COLOR_BLUE,"vehicleid - %d, ispassenger - %d",vehicleid,ispassenger);
	if(GetPVarInt(playerid, #InJobZD) == 1 && GetPVarInt(playerid, #useitemjob) == 1)
	{
		SendClientMessage(playerid, COLOR_REDD, !"Tqven Chamoagdet Nawili!");
		if(IsPlayerAttachedObjectSlotUsed(playerid, 0)) RemovePlayerAttachedObject(playerid, 0);
		if(IsPlayerAttachedObjectSlotUsed(playerid, 1)) RemovePlayerAttachedObject(playerid, 1);
		DisablePlayerCheckpoint(playerid);
		DeletePVar(playerid, #useitemjob);
		ClearAnimations(playerid);
		SetPVarInt(playerid,"RemCheckpointSklad",CheckPointForPlayer(playerid, 1691.5590,-1998.3466,14.1570,1.5));
	}
	if(PlayerInfo[playerid][pAdmin]) {
		if (veh_info [ vehicleid - 1 ] [ v_type ] == vehicle_type_player ) {
			foreach(new i : Player)
			{
				if(veh_info [ vehicleid - 1 ] [ v_owner ] == PlayerIDs[i]) SCMF(playerid, COLOR_BLUE, "[Naxav] "W"Es Manqana Ekutvnis "P"%s", Name(i));
			}
		}
	}
    SetPlayerArmedWeapon(playerid,0);
	VehicleID[playerid] = vehicleid;
	if(!veh_info[vehicleid-1][v_locked]) AllowSeat[playerid] = ispassenger;
    else AllowSeat[playerid] = -1;
	if(veh_info[vehicleid-1][v_locked])
	{
	    SCM(playerid, COLOR_LIGHTRED, "Transporti Daketilia!");
	    LockCar(vehicleid);
		GameTextForPlayer(playerid,"~w~VEHICLE ~r~LOCKED", 5000, 3);
		ClearAnim(playerid);
		TogglePlayerControllable(playerid, 0);
		setFreezePlayerForTime(playerid, 1);
	}
	if(usemats[playerid] == 1 || usemats[playerid] == 3)
	{
	    if(usemats[playerid] == 1) FI[3][fMaterials] += 250;
	    if(usemats[playerid] == 3) FI[19][fMaterials] += 250;
	    usemats[playerid] = 0;
		RemovePlayerAttachedObject(playerid, 1);
		SCM(playerid, COLOR_REDD,"Shen Daagde Yuti");
		SetPlayerSpecialAction (playerid, SPECIAL_ACTION_NONE);
	}
	if(usemats[playerid] == 5)
	{
	    DrugCount += 100;
	    usemats[playerid] = 0;
		RemovePlayerAttachedObject(playerid, 7);
		SCM(playerid, COLOR_REDD,"Tqven Chamoagdet Narkotikebi");
		SetPlayerSpecialAction (playerid, SPECIAL_ACTION_NONE);
	}
	if(usemats[playerid] == 6)
	{
	    MatsCount += 100;
	    usemats[playerid] = 0;
		RemovePlayerAttachedObject(playerid, 7);
		SCM(playerid, COLOR_REDD,"Tqven Chamoagdet Narkotikebi");
		SetPlayerSpecialAction (playerid, SPECIAL_ACTION_NONE);
	}
	SetVehicleParamsForPlayer(vehicleid, playerid, 0, veh_info[vehicleid-1][v_locked]);
    
	if(!ispassenger){
	    if(CarLic(vehicleid))
		{
			if(!PlayerInfo[playerid][pCarLic])
			{
			    if(TakingLesson[playerid] == 0){
					SCM(playerid, COLOR_WHITE, !"Tqven Ar Gaqvt Am Tipis Licenzia {828282}(( /gps >> [1] Mnishvnelovani Adgilebi >> [1] Martvis Skola ))");
