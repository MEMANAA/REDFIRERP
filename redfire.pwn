public VidahaZp()
{
    foreach(new i: Player)
    {
        if(PlayerInfo[i][pMember] == 0)
        {
            PlayerInfo[i][pFDays] = 0;
            PlayerInfo[i][pFHour] = 0;
            PlayerInfo[i][pFMinute] = 0;
        }
        if(PlayerInfo[i][pMember] > 0)
        {
            PlayerInfo[i][pFMinute] += 1;
            if(PlayerInfo[i][pFMinute] >= 60)
            {
				//TimeReportBot();
                PlayerInfo[i][pFHour] += 1;
                PlayerInfo[i][pFMinute] = 0;
                if(PlayerInfo[i][pFHour] >= 24)
                {
                    PlayerInfo[i][pFDays] += 1;
                    PlayerInfo[i][pFHour] = 0;
                }
            }
        }
        switch(PlayerInfo[i][pMember])
	    {
        //====================[Полиция]=========================
