CMD:takevkey ( playerid, params [] )
{
	if ( sscanf ( params, "u", params [ 0 ] ) ) return SendClientMessage ( playerid, col_gray, " Используйте: /takevkey [ид]" ) ;
	//if ( ! IsPlayerInRangeOfPoint ( playerid, 5, PlayerI [ params [ 0 ] ][ p_pos ] [ 0 ], p_t_info [ params [ 0 ] ][ p_pos ] [ 1 ], p_t_info [ params [ 0 ] ][ p_pos ] [ 2 ] ) || GetPlayerVirtualWorld ( params [ 0 ] ) != GetPlayerVirtualWorld ( playerid ) )return SendClientMessage ( playerid, col_gray," Игрок слишком далеко." ) ;
	if(!ProxDetectorS(5.0, playerid, params [ 0 ]) || GetPlayerVirtualWorld ( params [ 0 ] ) != GetPlayerVirtualWorld ( playerid ) ) return SendClientMessage ( playerid, col_gray," Игрок слишком далеко." ) ;

	if ( PlayerInfo [ params [ 0 ] ] [ v_key ] == 0 ) return SendClientMessage ( playerid, col_gray, " У игрока нет ключей от Вашего автомобиля." ) ;
	foreach ( new veh_id:player_vehicles[playerid])
	{
		if ( PlayerInfo [ params [ 0 ] ] [ v_key ] == veh_id )
		{
			new stringr [ 85 + ( -2 + MAX_PLAYER_NAME) ] ;
			PlayerInfo [ params [ 0 ] ] [ v_key ] = 0 ;
			format ( stringr, sizeof ( stringr ), "{"#cGN"}* {"#cBL"}%s {"#cWH"}забрал у Вас ключи от своего автомобиля.", Name (playerid) ) ;
			SendClientMessage ( params [ 0 ], -1, stringr ) ;
			format ( stringr, sizeof ( stringr ), "{"#cGN"}* {"#cWH"}Вы успешно забрали ключи от своего автомобиля у {"#cBL"}%s{"#cWH"}.", Name (params[0]) ) ;
			SendClientMessage ( playerid, -1, stringr ) ;
			return 1;
		}
		else continue ;
	}
	SendClientMessage ( playerid, col_gray, " У игрока нет ключей от Вашего автомобиля." ) ;
	return 1 ;
}



