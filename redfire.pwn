publics: GameStatusSelect(playerid, clickid)
{
	if(!GameStatus[playerid][0]) return 1;
//	SCMF(playerid, -1, " GameStatusSelect %d %d GameBox: %d [%d]", playerid, clickid, GameBox[playerid][CountStatus[playerid]], GameBox[playerid][CountStatus[playerid]]+48);
	new i;
	if(CountStatus[playerid] == 0) i = 9; 
	else if(CountStatus[playerid] == 1) i = 10;
	else if(CountStatus[playerid] == 2) i = 11;
	else if(CountStatus[playerid] == 3) i = 12;
	else if(CountStatus[playerid] == 4) i = 13;
	if(clickid == GameBox[playerid][CountStatus[playerid]]+49)
	{
		if(GameStatus[playerid][1] == 5){
			return 1;
		}	
		PlayerTextDrawColor(playerid, avia_mechanic_PTD[playerid][i], 8388863);
		PlayerTextDrawShow(playerid,avia_mechanic_PTD[playerid][i]);
		if(CountStatus[playerid] == GameStatus[playerid][1])
		{
			GameStatus[playerid][1] ++;
			if(GameStatus[playerid][1] != 5){
				SetTimerEx("GameAvia", 1000, false, "i", playerid);
				SetPVarInt(playerid, "GameEnd", 2);
			}	
			else
			{
				if ( e_quest_status [ playerid ] [ 1 ] == 1 )
				{
					e_quest_progress [ playerid ] [ 1 ] += 300 ;
					if ( e_quest_progress [ playerid ] [ 1 ] >= 5000 )
					{
						e_quest_status [ playerid ] [ 1 ] = 2 ;
						
						SendClientMessage ( playerid, COLOR_YELLOW, "Ежедневное задание Dasrulebuliо!" ) ;
						SendClientMessage ( playerid, -1, "(( Езжайте к месту спавна и подойтие к квестовому NPC ))" ) ;
					}
					update_equest_data ( playerid ) ;
				}
				GameTextForPlayer(playerid, "~g~+300$~n~~g~Task completed.", 3000, 6);
				PlayerPlaySound(playerid, 1054, 0.0, 0.0, 10.0);
				PlayerInfo[playerid][pSanZP] += 300;
				aviacount++;
				GameStatus[playerid][0] = 0, GameStatus[playerid][1] = 0, CountStatus[playerid] = 0;
				for(new lg = 0; lg < 5; lg++) GameBox[playerid][lg] = 0;
				for(new lg = 19; lg < 24; lg++) 
				{
					PlayerTextDrawColor(playerid, avia_mechanic_PTD[playerid][lg], 235802367);
					PlayerTextDrawShow(playerid,avia_mechanic_PTD[playerid][lg]);
				}
				SetTimerEx("GameAvia", 1000, false, "i", playerid);
				return 1;
			}
		}
		CountStatus[playerid] ++;
	}
	else
	{
		SCMF(playerid, -1, "GameStatus[0] %d GameStatus[1] %d CountStatus %d GameBox[][0] %d GameBox[d][1] %d GameBox[][2] %d GameBox[][3] %d GameBox[][4] %d", GameStatus[playerid][0], GameStatus[playerid][1], CountStatus[playerid], GameBox[playerid][0], GameBox[playerid][1], GameBox[playerid][2], GameBox[playerid][3], GameBox[playerid][4]);
		SetPVarInt(playerid, "GamePause", gettime()+1);
		GameTextForPlayer(playerid, "~r~Error! Try again.", 3000, 6);
		PlayerPlaySound(playerid, 1055, 0.0, 0.0, 10.0);
		ApplyAnimation(playerid,"MISC","plyr_shkhead",4.1,0,0,0,0,0,1);
		GameStatus[playerid][0] = 0, GameStatus[playerid][1] = 0, CountStatus[playerid] = 0;
		for(new lg = 0; lg < 5; lg++) GameBox[playerid][lg] = 0;
		PlayerTextDrawColor(playerid, avia_mechanic_PTD[playerid][i], COLOR_REDD);
		PlayerTextDrawShow(playerid,avia_mechanic_PTD[playerid][i]);	

		for(new lg = 19; lg < 24; lg++) 
		{
			PlayerTextDrawColor(playerid, avia_mechanic_PTD[playerid][lg], 235802367);
			PlayerTextDrawShow(playerid,avia_mechanic_PTD[playerid][lg]);
		}	

		if(GetPVarInt(playerid, "GameEnd") != 2) SetTimerEx("GameAvia", 1000, false, "i", playerid);
		SetPVarInt(playerid, "GameEnd", 1);
	}
	return 1;
}
