stock RefreshInv(playerid, slot)
{
	if(PlayerInfo[playerid][pInventoryKolvo][slot] > 0)
	{
		PlayerInfo[playerid][pInventoryKolvo][slot] -= 1;
		if(PlayerInfo[playerid][pInventoryKolvo][slot] == 0)
		{
			PlayerInfo[playerid][pInventoryKolvo][slot] = 0;
			PlayerInfo[playerid][pInventory][slot] = 0;
		}
		SaveInventory(playerid);
	}
	ClickInv[playerid] = -1;
	PlayerTextDrawBackgroundColor(playerid, inv_PTD[playerid][slot], 218959359);
	InvUpdate(playerid, slot, PlayerInfo[playerid][pInventory][slot]);
	return true;
}

stock GetInvSet(playerid)
{
	new num;
	for(new i = 0; i < 46; i++)
	{
		if(39 <= i <= 44) continue;
		if(PlayerInfo[playerid][pInventory][i] != 0) num++;
	}
	return num;
}
stock SaveInventory(playerid)
{
	new saveinv[800], saveinv_kolvo[800];
	for(new i = 0; i < 46; i++)
	{
		format(saveinv, 800, "%s%d,", saveinv, PlayerInfo[playerid][pInventory][i]);
		format(saveinv_kolvo, 800, "%s%d,", saveinv_kolvo, PlayerInfo[playerid][pInventoryKolvo][i]);
	}
	string[0] = EOS;

	mysql_format(connects, string, 256,"UPDATE `accounts` SET `Item` = '%s' WHERE `Name` = '%s'", saveinv, Name(playerid));
	mysql_tquery(connects, string, "", "" ) ;

	string[0] = EOS;
	mysql_format(connects, string, 256,"UPDATE `accounts` SET `ItemKolvo` = '%s' WHERE `Name` = '%s'", saveinv_kolvo, Name(playerid));
	mysql_tquery(connects, string, "", "" );
	return true;
}
stock GetInvent(playerid, slot)
{
	new colvo = 0;
	for(new i = 0; i < 46; i++)
	{
		if(PlayerInfo[playerid][pInventory][i] != slot) continue;
		colvo += PlayerInfo[playerid][pInventoryKolvo][i];
	}
	return colvo;
}
stock DeleteItem(playerid, itemid, amount)
{
	new item_amount = amount;
	for(new i = 0; i < 46; i++)
	{
		if(PlayerInfo[playerid][pInventory][i] != itemid) continue;
		if(PlayerInfo[playerid][pInventory][i] == itemid && item_amount > 0)
		{
			if(PlayerInfo[playerid][pInventoryKolvo][i] >= amount)
			{
				new new_amount = PlayerInfo[playerid][pInventoryKolvo][i];
				PlayerInfo[playerid][pInventoryKolvo][i] -= item_amount;
				item_amount -= new_amount;
				if(PlayerInfo[playerid][pInventoryKolvo][i] <= 0)
				{
					PlayerInfo[playerid][pInventoryKolvo][i] = 0;
					PlayerInfo[playerid][pInventory][i] = 0;
				}
			}
			else
			{
				item_amount -= PlayerInfo[playerid][pInventoryKolvo][i];
				PlayerInfo[playerid][pInventoryKolvo][i] = 0;
				PlayerInfo[playerid][pInventory][i] = 0;
			}
		}
	}
	SaveInventory(playerid);
	return 1;
}
stock EmptyMessage(const stringr[])
{
    new num, size = strlen(stringr);
        
	for(new i=0; i<size; i++)
	{
		switch(stringr[i])
		{
			case ' ', '\t', '\r', '\n': num++;
		}
	}

	if(num == size) return 1;
    
	return 0;
}
