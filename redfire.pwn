    }
    else if (PlayerInfo[playerid][pLeader] == 2 || PlayerInfo[playerid][pMember] == 2 && PlayerInfo[playerid][pRank] >= 9)
    {
        if(IsAArm(params[0]) && PlayerInfo[params[0]][pRank] >= 10 || IsACop(params[0]) && PlayerInfo[params[0]][pRank] >= 5) { ftext = "FBI"; ChosenSkin[params[0]] = !t_gen ? 286 : 141; }
