stock ShowItog(idx)
{
	new itog[3], string_[128];
	itog[0] = -1,
	itog[1] = -1,
	itog[2] = -1;
  	foreach(Player,i)
  	{
		if(GetPVarInt(i,"BoneStol")-1 == idx && GetPVarInt(i,"BoneStol_") > 1 && GetPVarInt(i,"BoneStol_") > itog[0])
		{
			itog[0] = GetPVarInt(i,"BoneStol_");
			itog[1] = i;
		}
		if(GetPVarInt(i,"BoneStol")-1 == idx && GetPVarInt(i,"BoneStol_") > 1 && i != itog[1] && GetPVarInt(i,"BoneStol_") == itog[0])
		{
			itog[2] = i;
		}
	}
	if(itog[2] > -1)
	{
	    foreach(Player,i)
	    {
		    if(GetPVarInt(i,"BoneStol")-1 == idx && GetPVarInt(i,"BoneStol_") > 1 && GetPVarInt(i,"BoneStol_") == itog[0])
			{
				SetPVarInt(i,"BoneStol_",1);
				BoneInfo[idx][GameStart] = 20;
				SendClientMessage(i,0xDA70D6FF,"{FF4B0F}Вы попали в следующий раунд. У вас есть 20 секунд, чтобы бросить кости");
			}
		    else if(GetPVarInt(i,"BoneStol")-1 == idx)
			{
			    DeletePVar(i,"BoneStol_");
			}
		}
		UpdateBone(idx);
	}
	else if(itog[1] > -1)
	{
	  	format(string_,sizeof(string_),"{FF4B0F}%s побеждает. Количество очков: %d. Выигрыш: %d",PlayerInfo[itog[1]][pName],itog[0],BoneInfo[idx][bBank]-(BoneInfo[idx][bBank]/25));
	  	if(BoneInfo[idx][Crupie] != INVALID_PLAYER_ID) PlayerInfo[BoneInfo[idx][Crupie]][pZP] += BoneInfo[idx][bBank]/1000;
	  	PlayerInfo[itog[1]][pCash] += BoneInfo[idx][bBank]-(BoneInfo[idx][bBank]/25);
		new money_percent = BoneInfo[idx][bBank]/25-(BoneInfo[idx][bBank]/1000);
	  	FI[7][fMoney] += money_percent/2;
		totpayday[0] += money_percent/2;
		BizzInfo[enterbiz[itog[1]]][b2Till] += money_percent/2;
	  	UpdateTableLog(PlayerInfo[itog[1]][pName], "-1", "затащил в казино (игра в 5ом)", BoneInfo[idx][bBank]-(BoneInfo[idx][bBank]/25));
	  	BoneInfo[idx][GameStart] = 0;
		BoneInfo[idx][Bet] = 0;
		BoneInfo[idx][bBank] = 0;
		foreach(Player,i)
		{
	   		if(GetPVarInt(i,"BoneStol")-1 == idx)
	   		{
	    		SendClientMessage(i,0xDA70D6FF, string_);
	    		DeletePVar(i,"BoneStol_");
	   		}
	   		for(new i_ = 0; i_ != 5; i_++)
			{
				if(BoneInfo[idx][Gamer][i_] == i)
				{
					BoneInfo[idx][Gamer][i_] = INVALID_PLAYER_ID;
				}
			}
	  	}
	  	UpdateBone(idx);
	}
  	return 1;
}
