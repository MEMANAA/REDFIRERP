stock SearchingHit(playerid)
{
    new d,stringo[128];
    for new i=0;i<sizeof string;i++ do string[i]=EOS;
    format(stringo, sizeof(stringo), "{FFFF00}Выберите и введите номер жертвы\n\n{FFFFFF}"),strcat(string, stringo);
	foreach(new i : Player)
	{
	    if(IsPlayerConnected(i))
		{
	        if(PlayerInfo[i][pHeadValue] > 0 && ContractInfo[i][cHitman] == INVALID_PLAYER_ID)
	        {
	            d++;
			    if(d > 0 && d < 5) format(stringo, sizeof(stringo), "[{32CD32}%d{FFFFFF}] - Номер : %d ~ [{FFFF00}%s (%d $){FFFFFF}]\n",d,i,PlayerInfo[i][pName],PlayerInfo[i][pHeadValue]),strcat(string, stringo);
	        }
		}
	}
	if(d == 0) return ShowPlayerDialogEx(playerid,5232,DIALOG_STYLE_MSGBOX, "Доступные контракты","Нет доступных контрактов","Ок","");
	ShowPlayerDialogEx(playerid,5230,DIALOG_STYLE_INPUT, "Доступные контракты",string,"Datanxmeba","Ukan");
	return true;
}
