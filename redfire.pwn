    if(hittype < 0 || hittype > 4) return 0;
	if ((hittype == BULLET_HIT_TYPE_NONE) && GetPlayerDistanceFromPoint(playerid, fX, fY, fZ) > 340.0 && (fX != 0.0 && fY != 0.0 && fZ != 0.0)) return false;
	if (
	(hittype == BULLET_HIT_TYPE_PLAYER && ! IsPlayerConnected(hitid)) || 
	(hittype == BULLET_HIT_TYPE_VEHICLE && !IsValidVehicle(hitid)) || 
	(hittype == BULLET_HIT_TYPE_OBJECT && !IsValidVehicle(hitid)) || 
	(hittype == BULLET_HIT_TYPE_PLAYER_OBJECT && !IsValidPlayerObject(playerid, hitid))
	) return false;  
	if (weaponid < 22 || weaponid > 38) return 0;
    if(weaponid > 46)
	{
		KickAC ( playerid, 22, "no_fire_gun" ) ;
		return 0;
	}
    if(!((22 <= weaponid <= 34) || weaponid == 38))
    {
		KickAC ( playerid, 20, "no_fire_gun" ) ;
		return 0;
	}
	if(IsPlayerInAnyVehicle(playerid) && weaponid == WEAPON_DEAGLE) return SetPlayerArmedWeapon(playerid, 0);
	if( hittype == BULLET_HIT_TYPE_PLAYER && (BadFloat(fX) || BadFloat(fY) || BadFloat(fZ)) )
	{
		KickAC ( playerid, 345, "crasher" );
	    return 0;
	}
	new slot = GetWeaponSlot ( weaponid ) ;
	if(hittype == BULLET_HIT_TYPE_PLAYER || hittype == BULLET_HIT_TYPE_VEHICLE)
	{
		if ((floatabs(fX) >= 10.0) || (floatabs(fY) >= 10.0) || (floatabs(fZ) >= 10.0)|| (floatabs(fX) <= -10.0) || (floatabs(fY) <= -10.0) || (floatabs(fZ) <= -10.0) )
		{
			return 0;
		}
	}
	if(weaponid > 47 && hittype == 0 && fX == 0.0000 && hitid == 0 && fY == 0.0000 && fZ == 0.0000)
	{
		KickAC(playerid, 343, "fx == 0.0000");
		return 0;
	}
	new Float: pos_x, Float:pos_y, Float:pos_z ;
	GetPlayerPos ( playerid, pos_x, pos_y, pos_z ) ;
	new Float:pos_px, Float:pos_py, Float:pos_pz, Float:pos_sx, Float:pos_sy, Float:pos_sz;
	GetPlayerLastShotVectors(playerid, pos_px, pos_py, pos_pz, pos_sx, pos_sy, pos_sz);
	if ( pos_z == pos_pz ) return KickAC ( playerid, 345, "damager" ), 0;
	switch(hittype)
	{
		case BULLET_HIT_TYPE_PLAYER:
        {
            if ( fX == 0.0 && fY == 0.0 && fZ == 0.0 ) return KickAC ( playerid, 346, "silent aim" ), 0;

            if ( !IsPlayerInAnyVehicle ( hitid ) && GetPlayerSurfingVehicleID ( hitid ) == 0xFFFF && GetPlayerSurfingObjectID ( hitid ) == 0xFFFF && !IsPlayerInRangeOfPoint ( hitid, 5.0, pos_sx, pos_sy, pos_sz ) )
			{
				if ( ++ playerWarningProAim { playerid } > 10 ) return KickAC ( playerid, 347, "silent aim" ), 0;
			}
			else playerWarningProAim{playerid} = 0;

            new Float:ppos_z;
		 	GetPlayerPos(playerid, ppos_z, ppos_z, ppos_z);
			if(floatabs(ppos_z - pos_pz) < 0.01)
			{
					if(++playerWarningAim0{playerid} > 2) return KickAC ( playerid, 348, "silent aim" ), 0;
			}
			else playerWarningAim0{playerid} = 0;

			new Float:hpos_z;
		 	GetPlayerPos(hitid, hpos_z, hpos_z, hpos_z);
		 	if(floatabs(hpos_z - pos_sz) < 0.01)
			{
				if(++playerWarningAim3{playerid} > 2) return KickAC ( playerid, 349, "silent aim" ), 0;
			}
		 	else playerWarningAim3{playerid} = 0;
        }
        case BULLET_HIT_TYPE_VEHICLE: if(!GetVehicleModel(hitid)) return 0;
		case BULLET_HIT_TYPE_OBJECT: if(!IsValidObject(hitid)) return 0;
		case BULLET_HIT_TYPE_PLAYER_OBJECT: if(!IsValidPlayerObject(playerid, hitid)) return 0;
	}
	if(hittype == BULLET_HIT_TYPE_PLAYER && IsPlayerConnected(hitid))
    {

        new Float:fCameraCoordinates[3];
        new Float:fCameraFrontVector[3];
        new Float:fSiteCoordinates[2];
        new Float:fPlayerPosition[3];
        new Float:f3DDistance;
        new Float:fDistance;
        new Float:fDifference;

        GetPlayerCameraPos(playerid, fCameraCoordinates[0], fCameraCoordinates[1], fCameraCoordinates[2]);
        GetPlayerCameraFrontVector(playerid, fCameraFrontVector[0], fCameraFrontVector[1], fCameraFrontVector[2]);
        GetPlayerPos(hitid, fPlayerPosition[0], fPlayerPosition[1], fPlayerPosition[2]);

        f3DDistance = GetPlayerDistanceFromPoint(hitid, fCameraCoordinates[0], fCameraCoordinates[1], fCameraCoordinates[2]);

        fSiteCoordinates[0] = fCameraCoordinates[0] + fCameraFrontVector[0];
        fSiteCoordinates[1] = fCameraCoordinates[1] + fCameraFrontVector[1];

        fDistance = floatsqroot(floatpower(fSiteCoordinates[1] - fPlayerPosition[1], 2) + floatpower(fSiteCoordinates[0] - fPlayerPosition[0], 2));
        fDifference = f3DDistance - fDistance;

        if (fDifference < 0.97 && !IsPlayerDying(hitid) && GLS[19] != 1)
        {
			SCM(playerid, COLOR_RED, "Tqveni Nasroli Dablokilia (Shesadzloa Iyenebdet Chets)");
            SendWarningCheatMessageToAdmin(playerid,"Silent Aim");
            printf("%s Silent Aim [fDifference: %f]", Name(playerid), fDifference);
            return 0;

        }
    }
    if ( PlayerInfo [ playerid ] [ p_gun_slot ] [ slot ] == 0 && TeleportTime[playerid] < 1 )return KickAC ( playerid, 340, "DGun"), false ;
	/******************Подсчет выстрелов******************************/
	if ( PlayerInfo [ playerid ] [ p_gun_ammo ] [ slot ] <= -7 && GetPlayerWeapon ( playerid ) != 0 )return KickAC ( playerid, 300, "Gautavebeli Sabrdzolo Masala (OPWS)"), false ;
	PlayerInfo [ playerid ] [ p_gun_ammo ] [ slot ] -- ;
	pSpecture[playerid][sShout][0]++;
	pSpecture[playerid][sShout][1]++;
	if(hittype == 1 && hitid != INVALID_PLAYER_ID)
	{
		pSpecture[playerid][sShout][2]++;
		pSpecture[playerid][sShout][3]++;
	}
	new timertest = GetTickCount()-DamageTimer[playerid];
	DamageTimer[playerid] = GetTickCount();
	if(timertest < 250 && weaponid == 24 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
	{
