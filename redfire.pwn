	}
	else
	{
		if(door_mp == 1)
		{
			if(derby_count == 19) return SendClientMessage(playerid, 0xCECFCEff, "Места на МП Двери заполнены");
			derby_count ++;
			new r = random(sizeof(SpawnCoords));
