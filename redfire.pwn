			if(!PlayerToPoint(10.0, playerid, -1987.3850,1112.7073,54.2547)) return SCM(playerid, COLOR_GREY, "Вы не в церкви!");
			if(IsPlayerConnected(ProposeOffer[playerid]))
			{
				if(ProxDetectorS(10.0, playerid, ProposeOffer[playerid]))
				{
					GetPlayerName(ProposeOffer[playerid], giveplayer, sizeof(giveplayer));
					SCMF(playerid, 0x6495EDFF, "Вы приняли предложение от %s's", giveplayer);
					SCMF(ProposeOffer[playerid], 0x6495EDFF, "%s приняла Ваш запрос быть Вашей Женой.", Name(playerid));
					strmid (PlayerInfo[playerid][pMarriedTo],giveplayer, 0, strlen(giveplayer), 24);
					strmid (PlayerInfo[ProposeOffer[playerid]][pMarriedTo],Name(playerid), 0, strlen(Name(playerid)), 24);
					//PlayerInfo[ProposeOffer[playerid]][pCash] -= 100000;
