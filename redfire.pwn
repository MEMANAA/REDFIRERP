CMD:fill(playerid, params[])
{
    if(sscanf(params, "i", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /fill [кол-во литров]");
	if(params[0] < 1 || params[0] > 100) return SCM(playerid, COLOR_GRAD1, "Нельзя меньше 1 л. и больше 100");
    for(new b = 1; b <= TotalBizz; b++)
	{
		if(PlayerToPoint(10.0, playerid, BizzInfo[b][bEntranceX], BizzInfo[b][bEntranceY], BizzInfo[b][bEntranceZ]) && BizzInfo[b][bType] == 4)
		{
		    if(!IsPlayerInAnyVehicle(playerid)) return SCM(playerid,COLOR_WHITE,"Вы не в машине");
		    if(BizzInfo[b][bProducts] <= 0 && BizzInfo[b][bOwned] == 1) return SCM(playerid,COLOR_GRAD1, "Заправка не работает"), BizzInfo[b][bProducts] = 0;
			if(BizzInfo[b][bLocked] == 1) return SCM(playerid,COLOR_GRAD1, "Заправка закрыта!");
			new car = GetPlayerVehicleID(playerid);
			if(veh_info [ car - 1 ] [ v_fuel ] + params[0] > 101) return SCM(playerid,COLOR_RED, "[Shecdoma] "W"Вместимость вашего бака не позволяет заправить более 100 литров!");
			if(PlayerInfo[playerid][pCash] < BizzInfo[b][bPrice]*params[0]) return SCM(playerid,COLOR_GRAD1, "У вас нет столько денег");
			if(!IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER || GetVehicleModel(car) == 481 || GetVehicleModel(car) == 509 || GetVehicleModel(car) == 510) return SCM(playerid,COLOR_YELLOW, "Вы не в автомобиле или этот транспорт нельзя заправить.");
			BizzInfo[b][b2Till] += BizzInfo[b][bPrice]*params[0];
			PlayerInfo[playerid][pCash] -= BizzInfo[b][bPrice]*params[0];
			if(BizzInfo[b][bOwned] == 1) BizzInfo[b][bProducts] -= 1*params[0];
			new Float:test = params[0];
			veh_info [ car - 1 ] [ v_fuel ] += test;
			SCMF(playerid, 0x33A5FFAA, "{33A5FF}Вы заправили свой автомобиль на {FFE13C}%d {33A5FF}литров. Цена за горючее составила: {FFE13C}%d$", params[0], BizzInfo[b][bPrice]*params[0]);
		}
	}
	return true;
}
CMD:charge(playerid)
{
    for(new b = 1; b <= TotalBizz; b++)
	{
		if(PlayerToPoint(10.0, playerid, BizzInfo[b][bEntranceX], BizzInfo[b][bEntranceY], BizzInfo[b][bEntranceZ]) && BizzInfo[b][bType] == 4)
		{
		    if(!IsPlayerInAnyVehicle(playerid)) return SCM(playerid,COLOR_WHITE,"Вы не в машине");
		    if(BizzInfo[b][bProducts] <= 0 && BizzInfo[b][bOwned] == 1) return SCM(playerid,COLOR_GRAD1, "Заправка не работает"), BizzInfo[b][bProducts] = 0;
			if(BizzInfo[b][bLocked] == 1) return SCM(playerid,COLOR_GRAD1, "Заправка закрыта!");
			new car = GetPlayerVehicleID(playerid);
			if(PlayerInfo[playerid][pCash] < BizzInfo[b][bElectroPrice]) return SCM(playerid,COLOR_GRAD1, "У вас нет столько денег");
			if(!IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER || GetVehicleModel(car) == 481 || GetVehicleModel(car) == 509 || GetVehicleModel(car) == 510) return SCM(playerid,COLOR_YELLOW, "Вы не в автомобиле или этот транспорт нельзя заправить.");
			if(!veh_info [ car - 1 ] [ v_electro ]) return SCM(playerid, COLOR_GRAD1, "У вашего тс не установлен электрический двигатель!");
			if(ActualElectroCar(car))
			{
				SCMF(playerid,COLOR_BLUE," Заряд электро-двигателя: {FFFFFF}Дата окончания заряда - %s",date("%dd.%mm.%yyyy в %hh:%ii:%ss", veh_info [ car - 1 ] [ v_electro ]));
				return 1;
			}	
			BizzInfo[b][b2Till] += BizzInfo[b][bElectroPrice];
			PlayerInfo[playerid][pCash] -= BizzInfo[b][bElectroPrice];
			if(BizzInfo[b][bOwned] == 1) BizzInfo[b][bProducts] -= 10;
			
			PlayerPlaySound(playerid, 17801, 0.0, 0.0, 0.0);
			SCM(playerid,COLOR_GREEN2,"[Informacia] "W"Вы зарядили электрический двигатель, теперь Вас ТС может работать без бензина 2 дня!");
			veh_info [ car - 1 ] [ v_electro ] = gettime()+(2*24*60*60);
			if ( veh_info [ car - 1 ] [ v_type ] == vehicle_type_player )
			{
				new query_string [ 66 ] ;
				format ( query_string, sizeof ( query_string ),"UPDATE `users_vehicles` SET `v_electro`='%d' WHERE `v_id` = '%d' LIMIT 1",
					veh_info [ car - 1 ] [ v_electro ],
					veh_info [ car - 1 ] [ v_id ] ) ;
				mysql_tquery ( connects, query_string ) ;
				print(query_string);
			}	
		}
	}
	return true;
}
