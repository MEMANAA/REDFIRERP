		}
		while(day_week >= 7) day_week -= 7;
		foreach(new i:Player) {
			if(!PlayerInfo[i][pLogin] || AntiCheatIsKickedWithDesync(i)) continue;
			if(PlayerInfo[i][pAdmin] > 0 && dostup[i] == 1) {
				static const Names_Days[7][22] = {"online_saturday","online_sunday","online_monday","online_tuesday","online_wednesday","online_thursday","online_friday"};
				cmdstr = "";
				format(cmdstr,sizeof(cmdstr),"UPDATE `admin` SET `%s` = '%d',`kicked` = '%d',`skicked` = '%d',`warned` = '%d',`offwarned` = '%d',`baned` = '%d',`offbaned` = '%d',`muted` = '%d', `ansed` = '%d', `rating` = '%d' WHERE admid = %i LIMIT 1",Names_Days[day_week], AdminInfo[i][admPlayTime],
				AdminInfo[i][admKicked],
				AdminInfo[i][admSKicked],
				AdminInfo[i][admWarned],
				AdminInfo[i][admOffWarned],
				AdminInfo[i][admBaned],
				AdminInfo[i][admOffBaned],
				AdminInfo[i][admMuted],
				AdminInfo[i][admAnsed],
				AdminInfo[i][admRating], AdminInfo[i][admID]);
				mysql_tquery(connects, cmdstr);
				AdminInfo[i][admPlayTime] = 0;
			}
		}
	}
	if(h == 00 && m == 00)
	{
	    if(getDayEx()==2)
		{
		    foreach(new i: Player)
			{
			    if(PlayerInfo[i][pLogin] || !PlayerInfo[i][pAdmin]) continue;
			    AdminInfo[i][admPlayTime] = 0;
			}
