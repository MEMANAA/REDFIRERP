publics: GiveMoney(playerid, amount, value[])
{
	if(playerid == -1 || playerid == INVALID_PLAYER_ID) return true;
	new stringer[40];
    if(amount > 0) format(stringer,sizeof(stringer),"~g~C:~w~ +%i$", amount);
    else format(stringer,sizeof(stringer),"~r~C:~w~ %i$", amount);
	GameTextForPlayer(playerid,stringer, 5000, 1);
	PlayerInfo[playerid][pCash] += amount;
	UpdatePlayerData(playerid, "pCash", PlayerInfo[playerid][pCash]);
	GivePlayerMoney(playerid, amount);
	UpdateTableLog2(Name(playerid), value, amount);
	return 1;
}
