	if(!IsPlayerConnected(params[0]) || (params[0] == INVALID_PLAYER_ID)) return SendClientMessage(playerid, COLOR_GRAD1, "Shecdoma! Motamashe Ver Moidzebna");
	if(GetString(Name(params[0]),"Sheldon_Cooper") || GetString(Name(params[0]),"Blake_Dexter") || GetString(Name(params[0]),"George_Osipov") || GetString(Name(params[0]),"Player")) return SCM(playerid, COLOR_GREY, "Этого администратора нельзя кикнуть!");
	if(PlayerInfo[params[0]][pAdmin] > PlayerInfo[playerid][pAdmin]) return err(!"[Shecdoma]: {AFAFAF}Администратор выше вас уровнем!");
	if(IsAIP(params[1])) return 0;
	if(CheckText(params[1], 1)) return AdminAntisliv(playerid, params[1]);
	format(cmdstr, sizeof(cmdstr), "Администратор: %s кикнул %s. Причина: %s",Name(playerid),Name(params[0]),params[1]);
	BroadCast(COLOR_LIGHTRED, cmdstr);
	Kick(params[0]);
	format(cmdstr, sizeof(cmdstr), "INSERT INTO `logs` (Text, Date) VALUES ('%s Кикнул: %s. Причина: %s', NOW())",Name(playerid),Name(params[0]),params[1]);
	mysql_tquery(connects, cmdstr);
	AdminInfo[playerid][admKicked]++;
	SetPVarInt(playerid, #ADMSLV, GetPVarInt(playerid, #ADMSLV) + 1);
	if(PlayerInfo[params[0]][pAdmin] && PlayerInfo[playerid][pAdmin] < 6)
    {
        BanAdmin(playerid);
        return true;
    }
	return true;
}
