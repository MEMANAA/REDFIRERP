CMD:demote(playerid, params[])
{
    new para1;
	new giveplayer[MAX_PLAYER_NAME];
	if(sscanf(params, "u",para1)) return  SendClientMessage(playerid, COLOR_WHITE, "Введите: /demote [id]");
	if(para1 == INVALID_PLAYER_ID) return SCM(playerid, COLOR_GREY, "Игрок не в игре!");
	GetPlayerName(para1, giveplayer, sizeof(giveplayer));
	if(PlayerInfo[para1][pLeader] >= 1) return SendClientMessage(playerid,COLOR_WHITE,"Вы не можете понизить лидера!");
	if(PlayerInfo[playerid][pMember] == 2 && PlayerInfo[playerid][pRank] >= 8 || PlayerInfo[playerid][pMember] == 7 && PlayerInfo[playerid][pRank] >= 5)
	{
		if(IsPlayerConnected(para1))
		{
			if(para1 != INVALID_PLAYER_ID)
			{
				switch(PlayerInfo[para1][pMember])
				{
					case 1,3,19,10,21:
					{
						PlayerInfo[para1][pRank] = PlayerInfo[para1][pRank]-1;
						if(PlayerInfo[para1][pRank]>0)
						{
							SCMF(para1, 0x6BB3FFAA, "Вы были понижены до %d ранга %s %s",PlayerInfo[para1][pRank], GetRank(playerid), Name(playerid));
							SCMF(playerid, 0x6BB3FFAA, "Вы понизили %s до %d ранга.", giveplayer,PlayerInfo[para1][pRank]);
						}
						else
						{
						    //SetWBook(params[0], true);
							PlayerInfo[para1][pMember] = 0;
							PlayerInfo[para1][pRank] = 0;
							PlayerInfo[para1][pLeader] = 0;
							PlayerInfo[para1][pJob] = 0;
							SetPlayerArmour(para1,0);
							DeletePVar(para1,"Fraction_Duty");
							SetPlayerColor(para1, 0xFFFFFF20);
							SetPlayerSkinEx(para1, PlayerInfo[para1][pChar][PlayerInfo[para1][pViborChar]]);
							SCMF(para1, 0x6BB3FFAA, "Вы были уволены %s %s",GetRank(playerid),Name(playerid));
							SCMF(playerid, 0x6BB3FFAA, "Вы уволили %s.", giveplayer);
							SendClientMessage(para1, 0x6BB3FFAA, "Вы снова гражданский");
							SendClientMessage(para1, 0x6BB3FFAA, "Рабочий день окончен");
							SendClientMessage(playerid,0x6BB3FFAA,"Вы уволили данного игрока");
						}
					}
					default: return SendClientMessage(playerid, COLOR_GREY, "[Shecdoma] Этот человек не армеец/полицейский!");
				}
			}
		}
	}
	return true;
}
CMD:giverank(playerid, params[])
{
	if(sscanf(params,"ud",params[0],params[1]))  return    SendClientMessage(playerid, COLOR_WHITE, "Введите: /giverank [id] [ранг]");
	if(params[0] == INVALID_PLAYER_ID) return SCM(playerid, COLOR_GREY, "Игрок не в игре!");
	if(!GetPVarInt(playerid, "Fraction_Duty")) return   SendClientMessage(playerid, COLOR_WHITE, "Вам необходимо быть в рабочей форме!");
	if(PlayerInfo[playerid][pRank] <= PlayerInfo[params[0]][pRank]) return SendClientMessage(playerid, COLOR_GREY, "[Shecdoma] Вы не можете повысить/понизить этого игрока");
	switch(PlayerInfo[playerid][pMember])
	{
		case 1,10,21:
		{
			if(PlayerInfo[playerid][pRank] < 8) return true;
			if((params[1] > 8 || params[1] < 1) && PlayerInfo[playerid][pLeader] == 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 8!"); return true; }
			if((params[1] > 9 || params[1] < 1) && PlayerInfo[playerid][pLeader] > 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 9!"); return true; }
		}
		case 3,19:
		{
			if(PlayerInfo[playerid][pRank] < 12) return true;
			if((params[1] > 11 || params[1] < 1) && PlayerInfo[playerid][pLeader] == 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 11!"); return true; }
			if((params[1] > 14 || params[1] < 1) && PlayerInfo[playerid][pLeader] > 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 14!"); return true; }
		}
		case 7:
		{
			if(PlayerInfo[playerid][pRank] < 4) return true;
			if((params[1] > 3 || params[1] < 1) && PlayerInfo[playerid][pLeader] == 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 3!"); return true; }
			if((params[1] > 5 || params[1] < 1) && PlayerInfo[playerid][pLeader] > 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 5!"); return true; }
		}
		case 8:
		{
			if(PlayerInfo[playerid][pRank] < 4) return true;
			if((params[1] > 3 || params[1] < 1) && PlayerInfo[playerid][pLeader] == 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 3!"); return true; }
			if((params[1] > 4 || params[1] < 1) && PlayerInfo[playerid][pLeader] > 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 4!"); return true; }
		}
		case 2,4,5,6,9,11,12,13,14,15,16,17,18,20:
		{
			if(PlayerInfo[playerid][pRank] < 7) return true;
			if((params[1] > 7 || params[1] < 1) && PlayerInfo[playerid][pLeader] == 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 7!"); return true; }
			if((params[1] > 9 || params[1] < 1) && PlayerInfo[playerid][pLeader] > 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 9!"); return true; }
		}
		case 24:
		{
			if(PlayerInfo[playerid][pRank] < 5) return true;
			if((params[1] > 4 || params[1] < 1) && PlayerInfo[playerid][pLeader] == 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 4!"); return true; }
			if((params[1] > 5 || params[1] < 1) && PlayerInfo[playerid][pLeader] > 0) { SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 5!"); return true; }
		}
	}
	if(!IsPlayerConnected(params[0])) return true;
	if(PlayerInfo[playerid][pMember] != PlayerInfo[params[0]][pMember]) return SendClientMessage(playerid, COLOR_GREY, "Данный игрок не в вашей фракции!");
	if(params[0] == INVALID_PLAYER_ID) return true;
	if(params[0] == playerid) return SendClientMessage(playerid, COLOR_GREY, "Вы указали свой ID");
	if (PlayerInfo[params[0]][pLeader] >= 1) return SendClientMessage(playerid, COLOR_GREY, "Вы указали ID лидера");
	PlayerInfo[params[0]][pRank] = params[1];
	SCMF(params[0], 0x6495EDFF, "Вы были повышены/понижены до %d ранга %s",params[1], Name(playerid));
	SCMF(playerid, 0x6495EDFF, "Вы повысили/понизили %s до %d ранга.", Name(params[0]),params[1]);
	return true;
}
