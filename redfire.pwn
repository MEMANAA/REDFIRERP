			format(cmdstr, sizeof(cmdstr), "INSERT INTO `logs_sms` (`Text`, `Date`) VALUES ('SMS sent %s to %s. Content: %s', NOW())", PlayerInfo[playerid][pName], PlayerInfo[params[0]][pName], params[1]);
			mysql_tquery(connects, cmdstr);
			return true;
		}
	}
	return true;
}
CMD:gg(playerid, params[])
{
    if(PlayerInfo[playerid][pAdmin] == 0 && PlayerInfo[playerid][pHelper] == 0) return true;
	if(sscanf(params, "d",params[0])) return SCM(playerid , COLOR_GREY, "Введите: /gg [id]");
	if(!IsPlayerConnected(params[0])) return true;
	SCMF(params[0], 0xFFCC00AA, "%s[%d] для %s[%d]: Приятной Вам игры на "NameServer"! Спасибо что Вы с нами =)",PlayerInfo[playerid][pName],playerid,PlayerInfo[params[0]][pName],params[0]);
	new good_game[126];
	format(good_game, sizeof(good_game), "%s[%d] пожелал приятной игры %s[%d]",PlayerInfo[playerid][pName], playerid, PlayerInfo[params[0]][pName], params[0]);
	ABroadCast(COLOR_GREY, good_game,1);
	HelperMessage(COLOR_GREY, good_game);
	PlayerPlaySound(params[0], 4203, 0.0, 0.0, 0.0);
	return true;
}
CMD:ans(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
	if(PlayerInfo[playerid][pHelper] <= 0) return true;
	if(sscanf(params, "us[200]", params[0], params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /ans [id игрока] [ответ]");
	if(!IsPlayerConnected(params[0])) return true;
	else if(IsAIP(params[1])) return 0;
	if(params[0] == playerid) return SCM(playerid, COLOR_GREY, "Нельзя ответить самому себе!");
	format(cmdstr, sizeof(cmdstr), "[Ответ] {D97700}[H] %s > %s:"W" %s", Name(playerid), Name(params[0]), params[1]);
 	SendSplitMessage(params[0], 0xF5DEB3AA, cmdstr);
   	SendAdminMessage(0xF5DEB3AA,cmdstr);
   	HelperMessage(0xF5DEB3AA,cmdstr);
   	PlayerInfo[playerid][pMpa] +=1;
	format(cmdstr, sizeof(cmdstr), "INSERT INTO `logs_report` (`Text`, `Date`) VALUES ('[ANS/Supports] Ответ от %s к %s [%s] Вопрос: %s',NOW())", PlayerInfo[playerid][pName], PlayerInfo[params[0]][pName], params[1], PlayerInfo[params[0]][pReportMessage]);
	mysql_tquery(connects, cmdstr);
   	return true;
}
