	SCMF(playerid, COLOR_GREEN2, "Вы выдали маты(+50000) и нарко(+15000) в оффлайне игроку %s", params[0]);
	format(cmdstr, sizeof(cmdstr), " Менеджер %s выдал маты/нарко в оффлайне игроку %s.",Name(playerid), params[0]);
    SendAdminMessage(COLOR_RED , cmdstr);
    WriteLog("ManagerLog.txt", cmdstr);
	return 1;
}		
CMD:givedm(playerid, params[])
{
    if(!PlayerInfo[playerid][Manager]) return true;
	if(sscanf(params, "u", params[0])) return SCM(playerid, COLOR_WHITE, "Введите: /givedm [playerID]");
