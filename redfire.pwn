CMD:tipster(playerid, params[])
{
	new param[40];
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(PlayerInfo[playerid][pMember] != 2)return SCM(playerid, COLOR_GREY, "Вы не полицейский/агент FBI");
	if(sscanf( params, "s[32]S()[64]", param, params ))
	{
		SCM(playerid, COLOR_WHITE, "Введите: /tipster [действие]. Доступные действия:");
		SCM(playerid, COLOR_WHITE, "  GET - Datanxmeba жучок.");
		SCM(playerid, COLOR_WHITE, "  SET - Прикрепить жучок к игроку");
		SCM(playerid, COLOR_WHITE, "  REMOVE - Деактивировать жучок");
		SCM(playerid, COLOR_WHITE, "  LISTEN - Начать прослушивание чата фракции");
		return true;
	}
	if(!strcmp(param, "get",true))
	{
	    if(PlayerInfo[playerid][pRank] < 6) return SCM(playerid,COLOR_ISPOLZUY,"Вам недоступна данная функция!");
	    if(GetPlayerVehicleID(playerid) >= fbicar[0] && GetPlayerVehicleID(playerid) <= fbicar[30]) {}
	    else return SCM(playerid,COLOR_GREY,"Вы должны находиться в автомобиле FBI");
	    if(tipster[playerid]) return SCM(playerid,COLOR_ISPOLZUY,"У вас уже есть жучок");
	    tipster[playerid] = 1;
	    SCM(playerid,COLOR_ISPOLZUY,"Вы взяли жучок! Используйте /tipster SET [id].");
	    format(cmdstr,sizeof(cmdstr),"%s взял жучок из автомобиля",Name(playerid));
	    SendRadioMessage(2,COLOR_ISPOLZUY, cmdstr);
	}
	else if(!strcmp(param, "set",true))
	{
	    new ammo;
	    if(PlayerInfo[playerid][pRank] < 6) return SCM(playerid,COLOR_ISPOLZUY,"Вам недоступна данная функция!");
	    if(sscanf( params, "u", ammo ) ) return SCM(playerid, COLOR_WHITE, "Введите: /tipster SET [playerid]");
	    if(!tipster[playerid]) return SCM(playerid,COLOR_ISPOLZUY,"У вас нет жучка");
	    if(!ProxDetectorS(2.0, playerid, ammo)) return SCM(playerid,COLOR_ISPOLZUY,"Игрок далеко от вас");
	    if(tipsteron != -1) return SCM(playerid,COLOR_ISPOLZUY,"Жучок уже прикреплен! Используйте /tipster REMOVE");
	    if(!PlayerInfo[ammo][pMember]) return SCM(playerid,COLOR_ISPOLZUY,"Игрок не состоит в организации");
	    tipster[playerid] = 0;
	    tipsteron = PlayerInfo[ammo][pMember];
	    tipsterplayer = ammo;
		format(cmdstr,sizeof(cmdstr),"%s прикрепил жучек к игроку %s. Используйте /tipster LISTEN для прослушивания",Name(playerid),Name(ammo));
	    SendRadioMessage(2,COLOR_ISPOLZUY, cmdstr);
	    SCM(playerid,COLOR_ISPOLZUY,"Вы прикрепили жучок к игроку! Используйте /tipster LISTEN");
	}
	else if(!strcmp(param, "remove",true))
	{
	    if(PlayerInfo[playerid][pRank] < 6) return SCM(playerid,COLOR_ISPOLZUY,"Вам недоступна данная функция!");
	    if(tipsteron == -1) return SCM(playerid,COLOR_ISPOLZUY,"Жучок не прикреплен!");
	    tipsteron = -1;
	    tipsterplayer = -1;
	    SCM(playerid,COLOR_ISPOLZUY,"Вы отключили жучок!");
	}
	else if(!strcmp(param, "listen",true))
	{
	    if(tipsteron == -1) return SCM(playerid,COLOR_ISPOLZUY,"жучок не прикреплен!");
	    if(!tipsterlisten[playerid])
	    {
	        tipsterlisten[playerid] = 1;
	        SCM(playerid,COLOR_ISPOLZUY,"Вы включили прослушивание!");
		}
		else
		{
		    tipsterlisten[playerid] = 0;
	        SCM(playerid,COLOR_ISPOLZUY,"Вы отключили прослушивание!");
		}
	}
	return true;
}
