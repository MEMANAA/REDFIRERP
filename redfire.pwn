CMD:uninvite(playerid, params[])
{
	if(sscanf(params, "us[64]",params[0],params[1])) return SCM(playerid, COLOR_GRAD2, " Используйте: /uninvite [id] [причина]");
	if(!IsPlayerConnected(params[0]) || (params[0] == INVALID_PLAYER_ID)) return SendClientMessage(playerid, COLOR_GRAD1, "Shecdoma! Motamashe Ver Moidzebna");
	if(PlayerInfo[playerid][pRank] == PlayerInfo[params[0]][pRank]) return SCM(playerid, COLOR_GREY, "[Shecdoma] Вы не можете уволить этого игрока");
	if(params[0] == playerid) return SCM(playerid,-1,"Себя невозможно уволить");
	//if(!IsPlayerConnected(params[0]) || (params[0] == INVALID_PLAYER_ID)) return SendClientMessage(playerid, COLOR_GRAD1, "Shecdoma! Motamashe Ver Moidzebna");
	if(PlayerInfo[params[0]][pMember] == PlayerInfo[playerid][pLeader] ||PlayerInfo[params[0]][pMember] == PlayerInfo[playerid][pMember])
	{
		if(!IsPlayerConnected(params[0])) return true;
		if(!CanUninvitePlayer(playerid,params[0])) return true;
		if(PlayerInfo[params[0]][pMember] <= 0) return true;
		//SetWBook(params[0], true);
		format(cmdstr, sizeof(cmdstr), "%s выгнал вас из организации. Причина:{FFFFFF} %s",Name(playerid),params[1]);
		SCM(params[0], 0x6495EDFF, cmdstr);
		format(cmdstr, sizeof(cmdstr), "Вы выгнали %s из организации с причиной:{FFFFFF} %s",Name(params[0]),params[1]);
		SCM(playerid, 0x6495EDFF, cmdstr);
		SCM(params[0], COLOR_WHITE, "Вы снова гражданский...");

		if(!IsAMafia(playerid) || !IsAGang(playerid)){
			format(cmdstr, sizeof(cmdstr), "[Организация] {FFFFFF}%s %s[%d]: выгнал %s. Причина: %s", GetRank(playerid), Name(playerid), playerid, Name(params[0]), params[1]);
		}
		else format(cmdstr, sizeof(cmdstr), "[Организация] {FFFFFF}%s %s[%d]: выгнал %s. Причина: %s", f_rank[PlayerInfo[playerid][pMember] - 1][PlayerInfo[playerid][pRank] - 1], Name(playerid), playerid, Name(params[0]), params[1]);	

		SendFamilyMessage(PlayerInfo[playerid][pMember], TEAM_AZTECAS_COLOR, cmdstr);
		WriteLog("UninviteLog.txt", cmdstr);
		add_jobinfo(params[0], params[1]);
		leave_team(params[0]);
		PlayerInfo[params[0]][pMember] = 0;
		PlayerInfo[params[0]][pMemberTime] = 0;
		PlayerInfo[params[0]][pLeader] = 0;
		PlayerInfo[params[0]][pRank] = 0;
		NullKD(params[0]);
		if(PlayerInfo[params[0]][pPhousekey] != 999) PlayerInfo[params[0]][pSpawnChange] = 1;
		SetPlayerArmour(params[0],0);
		DelGun(params[0]);
		DeletePVar(params[0],"Fraction_Duty");
		//PlayerSpawn(params[0]);
		DelGun(params[0]);
		SetPlayerColor(params[0], TEAM_HIT_COLOR);
		SetPlayerSkinEx(params[0],PlayerInfo[params[0]][pChar][PlayerInfo[params[0]][pViborChar]]);
		ShowPlayerDialogEx(params[0], d_spawn_select, DIALOG_STYLE_MSGBOX, ""P"Выбор спавна", ""W"Вы были уволены...\nХотите ли Вы переместиться на респавн?", "Да", "Нет");
	}
	return true;
}
CMD:uval(playerid, params[])
{
    if(PlayerInfo[playerid][pAdmin] < 4 || dostup[playerid] == 0) return true;
	new text[64];
	if(sscanf(params, "us[64]", params[0], text)) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /uval [id игрока] [причина]");
	if((PlayerInfo[playerid][pAdmin] < 3) && (PlayerInfo[params[0]][pLeader] > 0)) return SendClientMessage(playerid, COLOR_GRAD1, "Shecdoma! Вам недоступна эта функция");
	if(!IsPlayerConnected(params[0]) || (params[0] == INVALID_PLAYER_ID)) return SendClientMessage(playerid, COLOR_GRAD1, "Shecdoma! Motamashe Ver Moidzebna");
	if(PlayerInfo[params[0]][pMember] < 1) return SendClientMessage(playerid, COLOR_GRAD1, "Игрок не в организации");
	if(PlayerInfo[params[0]][pLeader] >= 1) return SCM(playerid,COLOR_WHITE,"Вы не можете уволить лидера! (Используйте /makeleader id 0)");
	//SetWBook(params[0], true);
 	format(cmdstr, sizeof(cmdstr), "Администратор %s[%d] уволил игрока %s[%d]. Причина: %s", PlayerInfo[playerid][pName], playerid, PlayerInfo[params[0]][pName], params[0], text);
 	SendAdminMessage(COLOR_GRAD1, cmdstr);
 	WriteLog("UninviteLog.txt", cmdstr);
	SCMF(params[0], 0x6495EDFF, "Администратор %s выгнал вас из организации. Причина: %s", PlayerInfo[playerid][pName], text);
	SCMF(playerid, 0x6495EDFF, "Вы выгнали %s из организации", PlayerInfo[params[0]][pName]);
	add_jobinfo(params[0], params[1]);
	leave_team(params[0]);
	PlayerInfo[params[0]][pMember] = 0;
	PlayerInfo[params[0]][pMemberTime] = 0;
	PlayerInfo[params[0]][pLeader] = 0;
	PlayerInfo[params[0]][pRank] = 0;
	PlayerInfo[params[0]][pJobHealLeader] = 0;
	PlayerInfo[params[0]][pJobHeal] = 0;
	NullKD(params[0]);
	if(PlayerInfo[params[0]][pPhousekey] != 999) PlayerInfo[playerid][pSpawnChange] = 1;
	DelGun(params[0]);
	SetPlayerArmour(params[0],0);
	DelGun(params[0]);
	DeletePVar(params[0],"Fraction_Duty");
	SetPlayerColor(params[0], TEAM_HIT_COLOR);
	SetPlayerSkinEx(params[0],PlayerInfo[params[0]][pChar][PlayerInfo[params[0]][pViborChar]]);
	ShowPlayerDialogEx(params[0], d_spawn_select, DIALOG_STYLE_MSGBOX, ""P"Выбор спавна", ""W"Вы были уволены...\nХотите ли Вы переместиться на респавн?", "Да", "Нет");
	return true;
}
