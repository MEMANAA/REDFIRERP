stock FixRusText(text[])
{
	new size = strlen(text);
	for ( new i = 0; i != size; i ++ )
    {
        switch ( text[i] )
        {
            case 'а': text[i] = 'a';
            case 'А': text[i] = 'A';
            case 'б': text[i] = '—';
            case 'Б': text[i] = 'Ђ';
            case 'в': text[i] = 'ў';
            case 'В': text[i] = '‹';
            case 'г': text[i] = '™';
            case 'Г': text[i] = '‚';
            case 'д': text[i] = 'љ';
