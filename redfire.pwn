CMD:switchskin(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(!IsAGang(playerid)) return true;
	if(forma[playerid] != 1) return SCM(playerid, COLOR_GRAD1, "У вас нет формы армейца");
	if(PlayerInfo[playerid][pSex] == 1) SetPlayerSkinEx(playerid,287);
	else SetPlayerSkinEx(playerid,191);
	SetPlayerColor(playerid, 0x51964DFF);
	forma[playerid] = 0;
	proverkaforma[playerid] = 1;
	new slot = ClickInv[playerid];
	if(slot != -1) RefreshInv(playerid, slot);
	else DeleteItem(playerid, 287, 1);
	return true;
}
