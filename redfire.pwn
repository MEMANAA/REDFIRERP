CMD:returntax(playerid)
{
	//SCMF(playerid, -1, "TAX_WORK_BID %d", GetPVarInt(playerid, #TAX_WORK_BID));
	if(!GetPVarInt(playerid, #TAX_WORK_BID)) return 1;
	new i = bizname[playerid][1];
	if (!PlayerToPoint(15, playerid,BizzInfo[i][bEntranceX],BizzInfo[i][bEntranceY],BizzInfo[i][bEntranceZ]))
	{
		SCMF(playerid, COLOR_GRAD2, "Вам нужно быть возле бизнеса %s!", BizzInfo[i][bMessage]);
		return 1;
	}
	SCM(playerid, COLOR_GREEN2, "[Minishneba] "W"Вы забрали налог с бизнеса, отнесите его в банк");
	SetPlayerCheckpoint(playerid, 1422.4884,-1702.0829,13.6658,5);
	return 1;
}	
CMD:returnmoney(playerid)
{
	new i = bizname[playerid][1];
	if (!PlayerToPoint(15, playerid,BizzInfo[i][bEntranceX],BizzInfo[i][bEntranceY],BizzInfo[i][bEntranceZ]))
	{	
		SCM(playerid, COLOR_GREY, "Необходимо быть возле бизнеса!");
		SCMF(playerid, COLOR_GREY, "(( Осталось %d из 5 долгов ))", quest_progress [ playerid ] [ 8 ]);
		return 1;
	}
	if ( quest_status [ playerid ] [ 8 ] == 1 )
	{
		quest_progress [ playerid ] [ 8 ] += 1 ;
		SCMF(playerid, COLOR_GREY, "Вы забрали долг у бизнеса, осталось %d из 5 долгов", quest_progress [ playerid ] [ 8 ]);
		if ( quest_progress [ playerid ] [ 8 ] != 5 )
		{
			new str[68], id = 0, Float: fDistance;
			string[0] = EOS;
			strcat ( string, "Бизнес\tСумма долга\tДистанция\n" ) ;
			new b = Random(1,TotalBizz), RandMoney = Random(1,1000000);
			id++;
			bizname[playerid][id] = b;
			fDistance = GetPlayerDistanceFromPoint(playerid, BizzInfo[b][bEntranceX], BizzInfo[b][bEntranceY], BizzInfo[b][bEntranceZ]);
			format(str,sizeof(str), "%s\t"GREEN"%i$\t{"#cBLUES"}%0.2f м.\n", BizzInfo[b][bMessage], RandMoney, fDistance);
			strcat(string,str);
			ShowPlayerDialogEx(playerid,8007,DIALOG_STYLE_TABLIST_HEADERS,""P"Список должников",string, "Datanxmeba", "Gaukmeba");
			SCMF ( playerid, COLOR_BLUE, "[Minishneba]"W" Чтобы забрать долг введите "P"/returnmoney"W" возле бизнеса "P"%s", BizzInfo[b][bMessage]) ;
		}	
		if ( quest_progress [ playerid ] [ 8 ] >= 5 )
		{
			quest_status [ playerid ] [ 8 ] = 2;
			ShowPlayerDialogEx(playerid,0,DIALOG_STYLE_MSGBOX,"{"#cBLUES"}Квест","{FFFF00}SMS: Узнал что ты уже собрал(а) все долги. Отличная работа! Отправитель: Джереми\n\
			{FFFFFF}(( Tqveni Jildos Misagebad Gamoiyenet: {33aaff}/quest"W" ))","Daxurva","");
			update_quest_data ( playerid ) ;	
		}
	}	
	return 1;
}
CMD:bizwithdraw(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(!IsPlayerConnected(playerid)) return true;
	new bouse = PlayerInfo[playerid][pPbiskey];
	if (bouse == 255)  return SCM(playerid, COLOR_GRAD2, "У вас нет бизнеса!");
	if(sscanf(params, "d", params[0]))
	{
		format(cmdstr, sizeof(cmdstr),  "На счету бизнеса %d $", BizzInfo[bouse][bTill]);
		SCM(playerid, COLOR_GRAD3, cmdstr);
	}
	SCM(playerid, COLOR_WHITE, "Введите: /bizwithdraw [сумма]");
	if(params[0] > BizzInfo[bouse][bTill] || params[0] < 1)  return SCM(playerid, COLOR_GRAD2, "На счету бизнеса нет столько денег!");
	if (!PlayerToPoint(100, playerid,BizzInfo[bouse][bEntranceX],BizzInfo[bouse][bEntranceY],BizzInfo[bouse][bEntranceZ]))
	{
		SCM(playerid, COLOR_GRAD2, "Вам нужно быть возле бизнеса!");
		return true;
	}
	else
	{
 		PlayerInfo[playerid][pCash] +=params[0];
 		UpdateTableLog2(Name(playerid), "Снятие со счёта бизнеса", params[0]);
		BizzInfo[bouse][bTill] -= params[0];
		format(cmdstr, sizeof(cmdstr), "Вы сняли %d $ со счёта вашего бизнеса. Остаток: %d",params[0],BizzInfo[bouse][bTill]);
		SCM(playerid, 0x6495EDFF, cmdstr);
		SaveBizz(bouse);
		return true;
	}
}
