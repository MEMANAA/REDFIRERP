CMD:bizwar(playerid, params[])
{
    if(!PlayerInfo[playerid][pLogin] || !IsAMafia(playerid) || PlayerInfo[playerid][pRank] < 6) return 1;
	new gang1 = -1;

	switch(PlayerInfo[playerid][pMember])
	{
		case 6: gang1 = MOROZ_YAKUZA;
		case 5: gang1 = MOROZ_LCN;
		case 14: gang1 = MOROZ_RM;
	}
	if(gang1 != -1)
	{
		if(fracmoroz[gang1] > gettime())
		{
			SCM(playerid,COLOR_RED,"Ваша мафия заморожена");
			return 1;
		}
	}
	new t; gettime(t);
	if(t == 01 || t == 02 || t == 03 || t == 04 || t == 05 || t == 06) return SendClientMessage(playerid, COLOR_RED, " Использовать команду можно только с 7:00 до 00:00");
    if(WarStart > gettime()) return SCM(playerid, COLOR_GREY, "Следующий захват можно начать в ?:30 минутах");
	if(sscanf(params, "d",params[0]) || params[0] < 1 || params[0] > 4) return SCM(playerid, COLOR_WHITE, !"Введите: /bizwar [id стрелы (1-4)]");
	for(new b = 1; b <= TotalBizz; b++)
	{
		if(PlayerToPoint(3, playerid,BizzInfo[b][bEntranceX], BizzInfo[b][bEntranceY], BizzInfo[b][bEntranceZ]))
		{
		    if(BizzInfo[b][bMafia] == PlayerInfo[playerid][pMember]) return SCM(playerid, COLOR_WHITE, !" Этот бизнес под Вашим контролем");
		    if(BizzInfo[b][bMafia] == 0)
			{
				BizzInfo[b][bMafia] = PlayerInfo[playerid][pMember];
				UpdateBizz(b);
				return SCM(playerid, COLOR_WHITE, !"Бизнес взят под контроль");

			}
			if(mafia_frac_id[0] != 0) return err(!"[Shecdoma]: {AFAFAF}В данное время уже идет война!");
			new gang = -1;

			switch(BizzInfo[b][bMafia])
			{
				case 6: gang = MOROZ_YAKUZA;
				case 5: gang = MOROZ_LCN;
				case 14: gang = MOROZ_RM;
			}
			if(gang != -1)
			{
				if(fracmoroz[gang] > gettime())
				{
					SCM(playerid,COLOR_RED,"Мафию которую вы хотите захватить невозможно");
					SCM(playerid,COLOR_RED,"Она заморожена администратором, ожидайте сообщения в чате когда заморозка будет снята");
					return 1;
				}
			}
		  	new string_[128], namezone[24];
			if(params[0] == 1) namezone = "HILLTOP FARMS";
			else if(params[0] == 2) namezone = "BLUEBERRY";
			else if(params[0] == 3) namezone = "PANOPTICON";
			else if(params[0] == 4) namezone = "HOLLOW FARM";
			
			format(string_, sizeof string_, "Вы забили стрелку %s. Бизнес: %s. Место встречи: %s [ID %d]",GetMafiaName(BizzInfo[b][bMafia]), BizzInfo[b][bMessage],namezone,params[0]);
			SendFamilyMessage(PlayerInfo[playerid][pMember],0x114D71AA, string_);
			SendFamilyMessage(PlayerInfo[playerid][pMember],0x114D71AA, "У вас есть 15 минут, чтобы надрать им зад!");
            format(string_, sizeof string_, "%s забила вам стрелку. Бизнес: %s. Место встречи: %s [ID %d]",GetMafiaName(PlayerInfo[playerid][pMember]),BizzInfo[b][bMessage],namezone,params[0]);
			SendFamilyMessage(BizzInfo[b][bMafia],COLOR_RED, string_);
			SendFamilyMessage(BizzInfo[b][bMafia],COLOR_RED, "У вас есть 15 минут, чтобы надрать им зад!");
			format(string_, sizeof string_,"%s %s инициировал(а) захват бизнеса", f_rank[PlayerInfo[playerid][pMember] - 1][PlayerInfo[playerid][pRank] - 1], PlayerInfo[playerid][pName]);
			SendFamilyMessage(PlayerInfo[playerid][pMember], TEAM_AZTECAS_COLOR, string_);
			SendFamilyMessage(BizzInfo[b][bMafia], TEAM_AZTECAS_COLOR, string_);
			
			format(string_, sizeof(string_), "[A] Начался захват между мафии %s и %s | Бизнес: %s [ID: %d]", GetMafiaName(PlayerInfo[playerid][pMember]), GetMafiaName(BizzInfo[b][bMafia]), BizzInfo[b][bMessage], params[0]);
			ABroadCast(0xFFC100FF, string_, 1);
			
			BizzInfo[b][bCapture] = PlayerInfo[playerid][pMember];
			ZoneID = params[0];
			mafia_frac_id[0] = PlayerInfo[playerid][pMember];
			mafia_frac_id[1] = BizzInfo[b][bMafia];
			kills_mafia[0] = 0;
			kills_mafia[1] = 0;
			ZoneTimer = 1000;
			if(ZoneID == 1) ZoneWar = GangZoneCreate(1002, -372, 1132, -272);
			else if(ZoneID == 2) ZoneWar = GangZoneCreate(-606, -208, -414, -20);
			else if(ZoneID == 3) ZoneWar = GangZoneCreate(-1494, -1596, -1384, -1436);
			else if(ZoneID == 4) ZoneWar = GangZoneCreate(16, -340, 180, -214);
			GangZoneShowForAll(ZoneWar, 0xB2B2B2AA);
			GangZoneFlashForAll(ZoneWar,COLOR_RED);
			MzCheckTimer = SetTimerEx("MzCheck",1000, true, "iii",b,PlayerInfo[playerid][pMember],BizzInfo[b][bMafia]);
			foreach(new idx: Player)
	 		{
	 		    if(PlayerInfo[idx][pMember] == mafia_frac_id[0])
	 		    {
					SetPlayerToTeamColor(idx);
					NullKD(idx);
				}
				else if(PlayerInfo[idx][pMember] == mafia_frac_id[1])
				{
					SetPlayerToTeamColor(idx);
					NullKD(idx);
				}
	 		}
	 		break;
		}
	}
	return 1;
}
publics: MzCheck(i,napad,vlad)
{
	if(ZoneID < 1 || ZoneID > 4)
	{
		KillTimer(MzCheckTimer);
		print("Error MzCheck <> ZoneID");
		return 1;
	}
	static const Float: coor_zone[4][4] = {
		{1002.0, -372.0, 1132.0, -272.0},
		{-606.0, -208.0, -414.0, -20.0},
		{-1494.0, -1596.0, -1384.0, -1436.0},
		{16.0, -340.0, 180.0, -214.0}
	};
	ZoneTimer--;
	
    foreach(new x: Player)
	{
		new y = PlayerInfo[x][pMember];
		if(y == napad || y == vlad)
		{
  			if(CaptureOn[x] == 0)
			{
				if(!PlayerInfo[x][settings][4])
				{
					for(new t; t < sizeof(TDCapt_TD); t++) TextDrawShowForPlayer(x, TDCapt_TD[t]);

					PlayerTextDrawColor(x, TDCapt_PTD[x][4], GetGangColor(napad));
					PlayerTextDrawColor(x, TDCapt_PTD[x][5], GetGangColor(vlad));
					PlayerTextDrawColor(x, TDCapt_PTD[x][2], GetGangColor(napad));
					PlayerTextDrawColor(x, TDCapt_PTD[x][3], GetGangColor(vlad));
					for(new t = 0; t < 6; t++)
					{
						PlayerTextDrawShow(x, TDCapt_PTD[x][t]);
					}
				}	
				else PlayerTextDrawShow(x, Capture[x]);	
				CaptureOn[x] = 1;
			}
			if(!PlayerInfo[x][settings][4])
			{
				if(ZoneTimer > 0) format(cmdstr,SMALL_SIZE,"ATTACK_TIME:~N~%s",Convert(ZoneTimer));
				else format(cmdstr,SMALL_SIZE,"~r~ATTACK_TIME:~N~%s",Convert(ZoneTimer));
				PlayerTextDrawSetString(x,TDCapt_PTD[x][1],cmdstr);
				format(cmdstr,MAX_STR,"%s~n~%d", GetMafiaName2(mafia_frac_id[0]), kills_mafia[0]);
				PlayerTextDrawSetString(x,TDCapt_PTD[x][2],cmdstr);
				format(cmdstr,MAX_STR,"%s~n~%d", GetMafiaName2(mafia_frac_id[1]), kills_mafia[1]);
				PlayerTextDrawSetString(x,TDCapt_PTD[x][3],cmdstr);

				format(cmdstr,160,"KILLS:_%d_____DEATH:_%d_____DMG:_%d", PlayerInfo[x][tKills], PlayerInfo[x][tDeaths], PlayerInfo[x][tDamage]);
				PlayerTextDrawSetString(x,TDCapt_PTD[x][0],cmdstr);
			}
			else
			{
				format(cmdstr,120,"~y~kills~n~~n~~g~%s:~w~ %i~n~~r~%s:~w~ %i", GetMafiaName2(mafia_frac_id[0]), kills_mafia[0], GetMafiaName2(mafia_frac_id[1]), kills_mafia[1]);
				PlayerTextDrawSetString(x,Capture[x],cmdstr);
			}	
		}
	}

	if(ZoneTimer == 600)
	{
	    SendFamilyMessage(napad,0x114D71AA, !"У вас осталось 10 минут чтобы решить, кому будет принадлежать бизнес!");
	    SendFamilyMessage(vlad,0x114D71AA, !"У вас осталось 10 минут чтобы решить, кому будет принадлежать бизнес!");
	}
	if(ZoneTimer == 180)
	{
	    SendFamilyMessage(napad,COLOR_RED, !"Пора Datanxmeba решение, кто получит контроль над бизнесом. Осталось 3 минуты.");
	    SendFamilyMessage(vlad,COLOR_RED, !"Пора Datanxmeba решение, кто получит контроль над бизнесом. Осталось 3 минуты.");
	}
	if(ZoneTimer < 1)

	{

		new count_on_zone[2],z = ZoneID-1;

        //foreach(PlayerTeam[vlad],x)
        foreach(new x: Player)
        {
            if(PlayerInfo[x][pMember] == vlad)
            {
                if(IsPlayerToSquare(x,coor_zone[z][0], coor_zone[z][1],coor_zone[z][2],coor_zone[z][3]) && GetPVarInt(x,"AFK_Time") < 2)
            	{
                	count_on_zone[0] = 1;
                	break;
            	}
            }
            
        }

        foreach(new x: Player)
        {
            if(PlayerInfo[x][pMember] == napad)
            {
                if(IsPlayerToSquare(x,coor_zone[z][0], coor_zone[z][1],coor_zone[z][2],coor_zone[z][3]) && GetPVarInt(x,"AFK_Time") < 2)
	            {
	                count_on_zone[1] = 1;
	                break;
	            }
	        }
        }

		new str_biz_name[64],str_control[68+sizeof(str_biz_name)];
		format(str_biz_name,sizeof(str_biz_name),"%s",BizzInfo[i][bMessage]);

        if(count_on_zone[0] == 1 && count_on_zone[1] == 1)
        {
            ZoneTimer = 120;
			format( str_control,sizeof(str_control),"Вам добавлено 2 минуты для решения: кто будет контролировать бизнес %s.",str_biz_name);
			SendFamilyMessage(napad, 0x00cc44FF,str_control);
			SendFamilyMessage(vlad, 0x00cc44FF,str_control);
            return 1;
        }
        if(count_on_zone[0] == 0)
        {
			format(str_control,sizeof(str_control),"Вы взяли под контроль бизнес %s",str_biz_name);
			SendFamilyMessage(napad, 0x114D71AA,str_control);
			format(str_control,sizeof(str_control),"Вы потеряли контроль над бизнесом %s",str_biz_name);
			SendFamilyMessage(vlad, 0xC56565AA,str_control);
			BizzInfo[i][bMafia] = napad;
			UpdateBizz(i);
			StopWar();
			format(cmdstr, sizeof(cmdstr), "[A] Мафия %s забрала бизнес [%s] у %s [KILLS: %d/%d]", GetMafiaName2(napad), str_biz_name, GetMafiaName2(vlad), kills_mafia[0], kills_mafia[1]);
			SendAdminMessage(COLOR_REDD, cmdstr);
        }
        else
        {
			format(str_control,sizeof(str_control),"Вы отстояли бизнес %s",str_biz_name);
           	SendFamilyMessage(vlad,0x114D71AA,str_control);
			format(str_control,sizeof(str_control),"Вам не удалось захватить бизнес %s",str_biz_name);
			SendFamilyMessage(napad,0xC56565AA,str_control);
			StopWar();
			format(cmdstr, sizeof(cmdstr), "[A] Мафия %s отстояла бизнес [%s] у %s [KILLS: %d/%d]", GetMafiaName2(vlad), str_biz_name, GetMafiaName2(napad), kills_mafia[1], kills_mafia[0]);
			SendAdminMessage(COLOR_REDD, cmdstr);
        }

	}
	return 1;
}

stock StopWar()
{
    foreach(new idx: Player)
	{
	    if(PlayerInfo[idx][pMember] == mafia_frac_id[0])
	    {
			CaptureOn[idx] = 0;
			for(new t = 0x0; t != 6;t++) PlayerTextDrawHide(idx, TDCapt_PTD[idx][t]);
			for(new t = 0x0; t != 11;t++) TextDrawHideForPlayer(idx, TDCapt_TD[t]);
			NullKD(idx);
		}
		if(PlayerInfo[idx][pMember] == mafia_frac_id[1])
		{
			CaptureOn[idx] = 0;
			for(new t = 0x0; t != 6;t++) PlayerTextDrawHide(idx, TDCapt_PTD[idx][t]);
			for(new t = 0x0; t != 11;t++) TextDrawHideForPlayer(idx, TDCapt_TD[t]);
			NullKD(idx);
		}
	}
    WarStart = gettime() + 5000;
	ZoneID = 0;
	ZoneTimer = 0;
	mafia_frac_id[0] = 0;
	mafia_frac_id[1] = 0;
	kills_mafia[0] = 0;
	kills_mafia[1] = 0;
	DeleteSVar("MzTime");
	GangZoneDestroy(ZoneWar);
	KillTimer(MzCheckTimer);
	mysql_tquery(connects, "SELECT * FROM  `accounts` WHERE  `mKills` > 0 ORDER BY `accounts`.`mKills` DESC LIMIT 0, 5", "TopCaptPay");
}
