	DestroyVehicle(vehicleid);
}
stock IsVehicleInQuad(Float:veh_x, Float:veh_y, Float:q_minx, Float:q_miny, Float:q_maxx, Float:q_maxy)
{
	if(veh_x >= q_minx && veh_x <= q_maxx && veh_y >= q_miny && veh_y <= q_maxy) return 1;
	else return 0;
}
publics: load_fine_vehicles(playerid)
{
	new rows;
	cache_get_row_count(rows);
	if(! rows) return SendClientMessage(playerid, COLOR_GREY, "У Вас нет арестованных транспортных средств!");
	string[0] = EOS;
	for(new i = 0; i < rows; i ++)
	{
		new veh_model,
			veh_plate[16],
			veh_id,
			fine_text[32],
			fine_pd[24],
			v_finer;
		cache_get_value_name_int(i, "v_id", veh_id);
		cache_get_value_name_int(i, "v_model", veh_model);
		cache_get_value_name(i, "v_plate", veh_plate, 16);
		cache_get_value_name(i, "v_fine_pd", fine_pd, 24);
		cache_get_value_name(i, "v_fine_text", fine_text, 32);
		cache_get_value_name_int(i, "v_fine", v_finer);
		new line_string[162];
		if(v_finer == 1){
			format(line_string, sizeof(line_string), ""W"%s (Nomeri: %s)\n", VehicleNames[veh_model - 400], veh_plate);
			strcat(string, line_string);
		}
		else {
			format(line_string, sizeof(line_string), ""W"%s (Nomeri: %s) "ORANGE"[Jarima %s. Mizezi: %s]\n", VehicleNames[veh_model - 400], veh_plate, fine_pd, fine_text);
			strcat(string, line_string);
		}
		format(line_string, 14, "lf_listid_%d", i);
		SetPVarInt(playerid, line_string, veh_id);
		SetPVarInt(playerid, "FINEID", v_finer);
	}
	if(GetPVarInt(playerid, #GetTickets) == 2){
		ShowPlayerDialogEx(playerid, d_fine, DIALOG_STYLE_LIST, ""P"Parkirebis Jarima", string, "Datanxmeba", "Daxurva");
		SetPVarInt(playerid, "GETCAR", 1);
		return 1;
	}
	if(!GetPVarInt(playerid, #GetTickets)) ShowPlayerDialogEx(playerid, d_fine, DIALOG_STYLE_LIST, ""P"Parkirebis Jarima", string, "Datanxmeba", "Daxurva");
	else{
		DeletePVar(playerid, #GetTickets);
		ShowPlayerDialogEx(playerid, 0, DIALOG_STYLE_LIST, ""P"Jarimebis Sia", string, "Daxurva", "");
	}
	return 1;
}
