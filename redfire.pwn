publics: joblist_callback(playerid, targetid) {
	new rows = cache_num_rows(),fields = 0;
	if(rows) {
		//new stringf[3000];
		string[0] = EOS;
		if(rows > 15) fields = rows - 15;
		format(string, 82, "Migebis Tarigi\tGatavisuflebis Tarigi\tOrgnizacia\tMizezi\n");
		for(new i = fields; i < rows; i++) {
			new ji_dateinvite[16],
				ji_dateuninvite[16],
				ji_reason[48],
				ji_fraction,
				//ji_warnings,
				ji_rang;

			new str[16 + 16 + 48 + 32 + 24];
			cache_get_value_name_int(i, "ji_fraction", ji_fraction);
			//cache_get_value_name_int(i, "ji_warnings", ji_warnings);
			cache_get_value_name_int(i, "ji_rang", ji_rang);

			cache_get_value_name(i, "ji_date_invite", ji_dateinvite, 16);
			cache_get_value_name(i, "ji_date_uninvite", ji_dateuninvite, 16);
			cache_get_value_name(i, "ji_reason", ji_reason, 48);
			
			format(str, sizeof(str), ""W"%s\t%s\t%s(%d)\t%s\n",
															ji_dateinvite,
															ji_dateuninvite,
															GetFractionName[ji_fraction],//FI[ji_fraction][fName],
															ji_rang,
															//ji_warnings,
															ji_reason);
			strcat(string, str);
		}
		new text[64];
		format(text, sizeof(text), ""P"Dasakmebis Istoria: %s", Name(playerid));
		ShowPlayerDialogEx(targetid, 0, DIALOG_STYLE_TABLIST_HEADERS, text, string, "Daxurva", "");
	}
	else return SCM(playerid, COLOR_RED, "Samushao Wignshi Chanawerebi Ar Aris");
	return 1 ;
}
stock add_jobinfo(playerid,const stringf[]) {
	new query[310];
	format(query, sizeof(query), "INSERT INTO `jobinfo` (`ji_uid`,`ji_date_invite`,`ji_date_uninvite`,`ji_reason`,`ji_fraction`,`ji_warnings`,`ji_rang`) SELECT `pID`, `fraction_date`, NOW(), '%s','%d','%d','%d' FROM `accounts` WHERE `pID` = '%d'",
	stringf, PlayerInfo[playerid][pMember], 0, PlayerInfo[playerid][pRank], PlayerIDs[playerid]);
	mysql_tquery(connects, query, "", "" ) ;
}
stock off_add_jobinfo(const name[],const stringf[]) {
	new strings[200];
	format(strings, sizeof(strings), "SELECT `pID`,`pMember`,`pRank` FROM `accounts` WHERE BINARY `Name` = '%s' LIMIT 1",name);
	new Cache:result = mysql_query(connects, strings);
	new rows = cache_num_rows();
	new id,member,/*warn,*/rank;
	if(rows) {
		cache_get_value_name_int(0, "pID",id);
		cache_get_value_name_int(0, "pMember",member);
		cache_get_value_name_int(0, "pRank",rank);
		//cache_get_value_name_int(0, "fwarn",warn);
	}
    cache_delete(result);

    new query[310];
	format(query, sizeof(query), "INSERT INTO `jobinfo` (`ji_uid`,`ji_date_invite`,`ji_date_uninvite`,`ji_reason`,`ji_fraction`,`ji_warnings`,`ji_rang`) SELECT `pID`, `fraction_date`, NOW(), '%s','%d','%d','%d' FROM `accounts` WHERE `pID` = '%d'",
	stringf, member, 0,rank, id);
	mysql_tquery(connects, query, "", "" ) ;
	return 1;
}
