CMD:mute(playerid, params[])
{
    if(PlayerInfo[playerid][pAdmin] < 1 || dostup[playerid] == 0) return true;
	if(sscanf(params, "uds[32]",params[0],params[1],params[2])) return	SendClientMessage(playerid, COLOR_WHITE, "Введите: /mute [playerid] [минуты] [причина]");
	else if(IsAIP(params[2])) return 0;
	else if(CheckText(params[2], 1)) return AdminAntisliv(playerid, params[2]);
	if(params[0] == INVALID_PLAYER_ID) return SCM(playerid, COLOR_GREY, "Игрок не в игре!");
	if(strval(params[2]) > 32) return SCM(playerid, CRED, "Запрещенно вводить в причину больше 32 символов!");
	if(PlayerInfo[params[0]][pMuted] > 0)
	{
		PlayerInfo[params[0]][pMuted] = 0;
		PlayerInfo[params[0]][pMuteTime] = 0;
		format(cmdstr, sizeof(cmdstr), "Администратор %s снял затычку игроку %s",PlayerInfo[playerid][pName],PlayerInfo[params[0]][pName]);
		BroadCast(COLOR_LIGHTRED, cmdstr);
		return true;
	}
	AdminInfo[playerid][admMuted]++;
	PlayerInfo[params[0]][pMuteTime] = params[1]*60;
	PlayerInfo[params[0]][pMuted] = 1;
	strmid (PlayerInfo[params[0]][pMuteReason], params[2], 0, strlen(params[2]), 32);
	format(cmdstr, sizeof(cmdstr), "Администратор %s выдал бан чата игроку %s на %d минут. Причина: %s",PlayerInfo[playerid][pName],PlayerInfo[params[0]][pName],params[1],params[2]);
	BroadCast(COLOR_LIGHTRED, cmdstr);
	SendClientMessage(params[0], COLOR_RED, "Вам дали бан чата!");
	return true;
}
CMD:fmute( playerid, params [ ] )
