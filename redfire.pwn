stock IsTextInvalid(const text[])
{
    if(strfind(text, "none", false) != -1) return true;
    if(strfind(text, "None", false) != -1) return true;
	if(strfind(text, "'", true) != -1) return true;
	if(strfind(text, "%", true) != -1) return true;
	if(strfind(text, "&", true) != -1) return true;
	if(strfind(text, "*", true) != -1) return true;
	if(strfind(text, "(", true) != -1) return true;
	if(strfind(text, ")", true) != -1) return true;
	return false;
}
stock is_text_invalid(const text[])
{
    new length = strlen(text);

    for(new i; i < length; i ++)
    {
        switch(text[i])
        {
            case 'А'..'Я', 'а'..'я', 'A'..'Z', 'a'..'z', '0'..'9', '_', '@', '.', ',', '[', ']', ' ': continue;
            default: return 1;
        }
    }
    return 0;
}
