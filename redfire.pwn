publics:GzCheck(const i, const napad, const vlad)
{
	new OnZONE[130][20];
	new zone = capture_now[capture_band( napad )];
	if(ZoneOnBattle[i] == 1)
	{
		if(GZSafeTime[i] == 0 && GetSVarInt("GzTime") != 1)
		{
			GZSafeTime[i] += 35;
  			SetSVarInt("GzTime", 1);
			SendFamilyMessage(GZInfo[i][gFrakVlad],0x00B953AA,"Dadga Dro Gadawyvitot Vin Aigebs Teritorias, Moedanze Erti Banda Unda Darches");
			SendFamilyMessage(GZInfo[i][gNapad],0xC56565AA,"Dadga Dro Gadawyvitot Vin Aigebs Teritorias, Moedanze Erti Banda Unda Darches");
		}
		if(GZSafeTime[i] > 0) GZSafeTime[i] --;
		foreach(new x: Player)
		{
			new faction = PlayerInfo[x][pMember];
			if(faction == GZInfo[zone][gNapad] || faction == GZInfo[zone][gFrakVlad])
			{
				if(PlayerToKvadrat(x,GZInfo[zone][gCoords][0], GZInfo[zone][gCoords][1],GZInfo[zone][gCoords][2],GZInfo[zone][gCoords][3]) && GetPVarInt(x,"AFK_Time") < 2 && GetPlayerVirtualWorld(x) == 0 && !PlayerInfo[x][pTempLeader])
				{
					if(ZoneOnBattle[i] == 1) OnZONE[i][faction] ++;
				}
			}
		}
		foreach(new x: Player)
		{
			new y = PlayerInfo[x][pMember];
			if(y == GZInfo[zone][gNapad] || y == GZInfo[zone][gFrakVlad])
			{
   				SetPlayerToTeamColor(x);
			    GangZoneFlashForPlayer ( x, GZInfo[zone][gID],0xFF0000AA) ;
				if(CaptureOn[x] == 0)
				{
					if(!PlayerInfo[x][settings][4])
					{
						for(new t; t < sizeof(TDCapt_TD); t++) TextDrawShowForPlayer(x, TDCapt_TD[t]);
						PlayerTextDrawColor(x, TDCapt_PTD[x][4], GetGangColor(GZInfo[zone][gNapad]));
						PlayerTextDrawColor(x, TDCapt_PTD[x][5], GetGangColor(GZInfo[zone][gFrakVlad]));
						PlayerTextDrawColor(x, TDCapt_PTD[x][2], GetGangColor(GZInfo[zone][gNapad]));
						PlayerTextDrawColor(x, TDCapt_PTD[x][3], GetGangColor(GZInfo[zone][gFrakVlad]));
						for(new t = 0; t < 6; t++) PlayerTextDrawShow(x, TDCapt_PTD[x][t]);
					}
					else PlayerTextDrawShow(x, Capture[x]);
					CaptureOn[x] = 1;
				}
				if(!PlayerInfo[x][settings][4])
				{
					if(GZSafeTime[i] > 0) format(cmdstr,SMALL_SIZE,"ATTACK_TIME:~N~%s",Convert(GZSafeTime[i]));
					else format(cmdstr,SMALL_SIZE,"~r~ATTACK_TIME:~N~%s",Convert(GZSafeTime[i]));
					PlayerTextDrawSetString(x,TDCapt_PTD[x][1],cmdstr);
					format(cmdstr,MAX_STR,"%s~n~%d",GetGangName(GZInfo[zone][gNapad]),CountOnZone[GZInfo[zone][gNapad]]);
					PlayerTextDrawSetString(x,TDCapt_PTD[x][2],cmdstr);
					format(cmdstr,MAX_STR,"%s~n~%d",GetGangName(GZInfo[zone][gFrakVlad]), CountOnZone[GZInfo[zone][gFrakVlad]]);
					PlayerTextDrawSetString(x,TDCapt_PTD[x][3],cmdstr);
					format(cmdstr,160,"KILLS:_%d_____DEATH:_%d_____DMG:_%d", PlayerInfo[x][tKills], PlayerInfo[x][tDeaths], PlayerInfo[x][tDamage]);
					PlayerTextDrawSetString(x,TDCapt_PTD[x][0],cmdstr);
				}	
				else
				{
					format(cmdstr,120,"~y~kills~n~~n~~g~%s:~w~ %i~n~~r~%s:~w~ %i", GetGangName(GZInfo[zone][gFrakVlad]), CountOnZone[GZInfo[zone][gFrakVlad]], GetGangName(GZInfo[zone][gNapad]), CountOnZone[GZInfo[zone][gNapad]]);
					PlayerTextDrawSetString(x,Capture[x],cmdstr);	
				}	

			}
		}
		new asd;
		for(new x; x < 20; x++) if(OnZONE[i][x] > 0 && IsInAllowedF(x) && GZSafeTime[i] <= 0) asd++;
		if(asd == 1)
		{
			for(new z = 0; z < 20; z++)
			{
				if(OnZONE[i][z] > 0)
				{
					new zx,zl;
					zx = z;
					zl = i;
					if(GZInfo[zone][gFrakVlad] == z)
					{
						FI[GZInfo[zone][gFrakVlad]][fRepute] ++;
						SendFamilyMessage(GZInfo[zone][gFrakVlad],0x00B953AA,"Tqven Daicavit Tqveni Teritoria");
						SendFamilyMessage(GZInfo[zone][gNapad],0xC56565AA,"Tqven Ver Moaxerxet Teritoriis Dakaveba");
						format(cmdstr, sizeof(cmdstr), "[A] Banda %s Icavda Teriotrias %s [KILLS: %d/%d]", GetGangName(GZInfo[zone][gFrakVlad]), GetGangName(GZInfo[zone][gNapad]), CountOnZone[GZInfo[zone][gFrakVlad]], CountOnZone[GZInfo[zone][gNapad]]);
						SendAdminMessage(COLOR_REDD, cmdstr);
						foreach(new y: Player)
						{
							if(PlayerInfo[y][pMember] == GZInfo[zone][gNapad] || PlayerInfo[y][pMember] == GZInfo[zone][gFrakVlad])
							{
								UpdateGangInfo(PlayerInfo[y][pMember]);
								CaptureOn[y] = 0;
								if(!PlayerInfo[y][settings][4])
								{
									for(new t = 0x0; t != 6;t++) PlayerTextDrawHide(y,TDCapt_PTD[y][t]);
									for(new t = 0x0; t != 11;t++) TextDrawHideForPlayer(y, TDCapt_TD[t]);	
								}
								else PlayerTextDrawHide(y, Capture[y]);
							}
						}
					}
					else
					{
						format(cmdstr, sizeof(cmdstr), "[A] Bandam %s Axali Teritoria Aigo %s [KILLS: %d/%d]", GetGangName(GZInfo[zone][gNapad]), GetGangName(GZInfo[zone][gFrakVlad]), CountOnZone[GZInfo[zone][gNapad]], CountOnZone[GZInfo[zone][gFrakVlad]]);
						SendAdminMessage(COLOR_REDD, cmdstr);
						SendFamilyMessage(GZInfo[zone][gNapad], 0x00B953AA,"Tqven Aiget Kontroli Axal Teritoriaze");
						FI[GZInfo[zone][gNapad]][fRepute] ++;
						for(new y = 0; y < sizeof(GZInfo); y++)
						{
							if(GZInfo[y][gFrakVlad] == 12) TotalGzB++;
							if(GZInfo[y][gFrakVlad] == 13) TotalGzV++;
							if(GZInfo[y][gFrakVlad] == 15) TotalGzG++;
							if(GZInfo[y][gFrakVlad] == 17) TotalGzA++;
							if(GZInfo[y][gFrakVlad] == 18) TotalGzR++;
						}
						new maxgz, gang;
						maxgz = TotalGzB, gang = 12;
						if(maxgz < TotalGzV)
						{
							maxgz = TotalGzV, gang = 13;
						}
						if(maxgz < TotalGzG)
						{
							maxgz = TotalGzG, gang = 15;
						}
						if(maxgz < TotalGzA)
						{
							maxgz = TotalGzA, gang = 17;
						}
						if(maxgz < TotalGzR)
						{
							maxgz = TotalGzR, gang = 18;
						}
						fracmoroz[8] = gang;
						UpdateMorozed();
						format(string,64, "Bariga"W"\nAkontrolebs: {33aaff}%s", GetGangName(fracmoroz[8]));
						UpdateDynamic3DTextLabelText(Priton3D, COLOR_BLUE, string);
						new stringer[128];
						switch(GZInfo[zone][gNapad])
						{
						case 12:
							{
								format(stringer, sizeof(stringer), "Axla Tqveni Banda Miigebs Yovel Saatshi %d $",TotalGzB*50);
								SendFamilyMessage(GZInfo[zone][gNapad], 0x00B953AA, stringer);
							}
						case 13:
							{
								format(stringer, sizeof(stringer), "Axla Tqveni Banda Miigebs Yovel Saatshi час %d $",TotalGzV*50);
								SendFamilyMessage(GZInfo[zone][gNapad], 0x00B953AA, stringer);
							}
						case 15:
							{
								format(stringer, sizeof(stringer), "Axla Tqveni Banda Miigebs Yovel Saatshi час %d $",TotalGzG*50);
								SendFamilyMessage(GZInfo[zone][gNapad], 0x00B953AA, stringer);
							}
						case 17:
							{
								format(stringer, sizeof(stringer), "Axla Tqveni Banda Miigebs Yovel Saatshi %d $",TotalGzA*50);
								SendFamilyMessage(GZInfo[zone][gNapad], 0x00B953AA, stringer);
							}
						case 18:
							{
								format(stringer, sizeof(stringer), "Axla Tqveni Banda Miigebs Yovel Saatshi час %d $",TotalGzR*50);
								SendFamilyMessage(GZInfo[zone][gNapad], 0x00B953AA, stringer);
							}
						}
						SendFamilyMessage(GZInfo[zone][gFrakVlad], 0xC56565AA,"Tqven Dakarget Kontroli Tqvens Ert-Ert Teritoriaze");
						SendFamilyMessage(GZInfo[zone][gNapad], 0xDEA002AA,"Top-5 Sauketeso Motamashe: {FFFFFF}/topcapt");
						SendFamilyMessage(GZInfo[zone][gFrakVlad], 0xDEA002AA,"Top-5 Sauketeso Motamashe: {FFFFFF}/topcapt");
						foreach(new y: Player)
						{
							if(PlayerInfo[y][pMember] == GZInfo[zone][gNapad] || PlayerInfo[y][pMember] == GZInfo[zone][gFrakVlad])
							{
								UpdateGangInfo(PlayerInfo[y][pMember]);
								CaptureOn[y] = 0;
								if(!PlayerInfo[y][settings][4])
								{
									for(new t = 0x0; t != 6;t++) PlayerTextDrawHide(y,TDCapt_PTD[y][t]);
									for(new t = 0x0; t != 11;t++) TextDrawHideForPlayer(y, TDCapt_TD[t]);	
								}
								else PlayerTextDrawHide(y, Capture[y]);
							}
						}
					}
					captures--;
					CountOnZone[GZInfo[zone][gNapad]] = 0;
					CountOnZone[GZInfo[zone][gFrakVlad]] = 0;
					capture_now[capture_band( GZInfo[zone][gNapad] )] = -1;
  					capture_now[capture_band( GZInfo[zone][gFrakVlad] )] = -1;
  					HOW[0] = 0.0, HOW[1] = 0.0, HOW[2] = 0.0;
  					HOW2[0] = 0.0, HOW2[1] = 0.0, HOW2[2] = 0.0;
					ClearTotalGz();
					ZoneOnBattle[zl] = 0;
					GZInfo[zl][gNapad] = 0;
					GZInfo[zl][gFrakVlad] = zx;
					GangZoneStopFlashForAll(GZInfo[zl][gID]);
					GangZoneHideForAll(GZInfo[zl][gID]);
					GangZoneShowForAll(GZInfo[zl][gID],GetGangZoneColor(zl));
					SaveGZ(zl);
					if(GetSVarInt("GzTime")) DeleteSVar("GzTime");
					if(captures == 0) mysql_tquery(connects, "SELECT * FROM  `accounts` WHERE  `tKills` > 0 ORDER BY `accounts`.`tKills` DESC LIMIT 0, 5", "TopCaptPay");//KillTimer(GzCheckTimer), GzCheckOn = 0;
					KillTimer(GZInfo[i][gTimer]);
					GZInfo[i][gTimer] = 0;
					break;
				}
			}
		}
	}
