stock AddItem(playerid, itemid, amount) // выдал Nivtebi в инвентарь
{
	if(GetInvSet(playerid) >= 36) return SendClientMessage(playerid, COLOR_GREY, "Ar Aris Sakmarisi Adgili Inventory Shi");
	for(new i = 0; i < 46; i++)
	{
		if(39 <= i <= 44) continue;
		if(PlayerInfo[playerid][pInventory][i] == itemid) continue;
		if(PlayerInfo[playerid][pInventory][i] != 0) continue;
		PlayerInfo[playerid][pInventory][i] = itemid;
		PlayerInfo[playerid][pInventoryKolvo][i] = amount;
		new str[350];
		format(str,sizeof(str),"[Inventory]"W" Daasamateblad (а) {33aaff}%s (%d шт)"W". Daawekit {33aaff}\"Y\"{ffffff} An Akrife {33aaff}/inv",ItemsInfo[itemid][ItemName],PlayerInfo[playerid][pInventoryKolvo][i]);
		SendClientMessage(playerid,COLOR_BLUE, str);
		SaveInventory(playerid);
		InvUpdate(playerid, i, PlayerInfo[playerid][pInventory][i]);
		break;
	}
	return true;
}
