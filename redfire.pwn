	new veh_id = GetPlayerVehicleID ( playerid ) ;
	if ( IsValidVehicle ( veh_id ) )
	{
		if ( veh_info [ veh_id - 1 ] [ v_owner ] != PlayerIDs [ playerid ] || veh_info [ veh_id - 1 ] [ v_type ] != vehicle_type_player ) return SendClientMessage( playerid, col_gray, "Вы должны находиться в Вашем личном транспорте!" ) ;
