CMD:at(playerid)
{
	if(!IsACop(playerid) && !GetPVarInt(playerid,"Fraction_Duty") && PlayerInfo[playerid][pRank] < 5) return SendClientMessage(playerid, 0xBEBEBEAA, "Es command tqventvis miuwdomelia!");
	new vehicle = GetPlayerVehicleID(playerid);
	new model = GetVehicleModel(vehicle);
	new targetVehicle, Float:targetVehicleDistance, closestVehicle, Float:closestVehicleDistance = 7.0, found = 0;
	new Float:tX, Float:tY, Float:tZ;
	if ((GetPlayerState(playerid) == PLAYER_STATE_DRIVER) && (model == 525)) 
    {
		while(targetVehicle < MAX_VEHICLES) 
		{
			GetVehiclePos(targetVehicle, tX, tY, tZ);
			targetVehicleDistance = GetVehicleDistanceFromPoint(vehicle, tX, tY, tZ);
			if(IsValidVehicle(targetVehicle) && (floatcmp(targetVehicleDistance, 7.0) <= 0) && (targetVehicle != vehicle) && (floatcmp(targetVehicleDistance, closestVehicleDistance) <= 0)) 
			{
				found = 1;
				closestVehicle = targetVehicle;
				closestVehicleDistance = targetVehicleDistance;
			}
			targetVehicle++;
		}
		if(found) 
		{
			if  (IsTrailerAttachedToVehicle(vehicle))
			{
				DetachTrailerFromVehicle(vehicle);
			}
			if ( veh_info [ closestVehicle - 1 ] [ v_type ] != vehicle_type_player) return SendClientMessage(playerid, 0xBEBEBEAA, "Вы пыаетесь оштрафовать не личный транспорт!");

			foreach(new i: Player)
			{
				if(IsPlayerInVehicle(i, closestVehicle))
				{
					SendClientMessage(playerid, 0xBEBEBEAA, "Вы этом транспорте есть человек!");
					continue;
				}
			}	
			AttachTrailerToVehicle(closestVehicle, vehicle);

			SendClientMessage(playerid, 0x0095ffFF, "[Диспетчер] {FFFFFF}Отправляйтесь на парковку San Andreas PD и передайте автомобиль на штрафстоянку.");
			SetPlayerGPS(playerid, 1605.0063,-1619.0231,13.5193);
			SetPVarInt(playerid, #GETVEH, closestVehicle);
		}
	}		
	return 1;
}	
