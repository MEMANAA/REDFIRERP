	}
	return true;
}
public OnPlayerExitVehicle(playerid, vehicleid)
{
    if(connect_mserver == 1) SCMF(playerid,COLOR_BLUE,"OnPlayerExitVehicle(playerid - %d, vehicleid - %d)",playerid, vehicleid);
    CheckVehicle[playerid] = gettime() + 2;
    VehicleID[playerid] = INVALID_VEHICLE_ID;
    veh_info [ vehicleid] [ v_driver ] = INVALID_PLAYER_ID ;
    
	if(GetPVarInt(playerid, "PlayerTaxi") > 0)
	{
		DeletePVar(playerid, "MellageTaxi");
		DeletePVar(playerid, "PlayerTaxi");
	}
	if(PEfir[playerid] != 255)
	{
		SCM(playerid, COLOR_WHITE, "Вы вышли из прямого эфира");
		PEfir[playerid] = 255;
	}
	if(Pefir[playerid] == 1 || Pefir[playerid] == 2)
	{
		PEfir[playerid] = 255;
		SCM(playerid, COLOR_WHITE, "Вы вышли из прямого эфира");
	}
	if(GetPVarInt(playerid, #SPEEDREDUCED)) DeletePVar(playerid, #SPEEDREDUCED);
	return true;
}
