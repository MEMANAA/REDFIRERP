publics: banhistory_callback ( playerid )
{
	new rows;
	cache_get_row_count(rows);
	if ( rows )
	{
		string [ 0 ] = EOS ;
		strcat ( string, "{"#cWH"}" ) ;
		for ( new j = 0 ; j < rows ; j ++ )
		{
			new admin_name [ MAX_PLAYER_NAME ],
				warn_reason [ 32 ],
				warn_date [ 22 ], line_string [ 98 ] ;


			cache_get_value_name ( j, "u_b_admin", admin_name) ;
			cache_get_value_name ( j, "u_b_reason", warn_reason) ;
			cache_get_value_name ( j, "u_b_date", warn_date) ;


			format ( line_string, sizeof ( line_string ), "%s\t%s\t%s\n", warn_date, admin_name, warn_reason ) ;
			strcat ( string, line_string ) ;
		}
		ShowPlayerDialog ( playerid, 0, DIALOG_STYLE_MSGBOX, "{"#cBLUES"}Список блокировок", string, "Daxurva", "" ) ;
	}
	if ( ! rows ) return SendClientMessage ( playerid, col_gray," Informacia о блокировках не найдена." ) ;
	return 1 ;
}
