	new scm_string [ 144 ] ;
	format ( scm_string, sizeof scm_string, "SELECT * FROM `bans` WHERE `u_b_name` = '%s'", params [ 0 ] ) ;
	mysql_tquery ( connects, scm_string, "banhistory_callback", "i", playerid ) ;
	return 1 ;
